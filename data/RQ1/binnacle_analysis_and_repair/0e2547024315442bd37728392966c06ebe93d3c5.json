  "repairedDockerfile": "FROM ubuntu:16.04\nMAINTAINER Gruntwork <info@gruntwork.io>\n\n# Install basic dependencies\nRUN DEBIAN_FRONTEND=noninteractive apt-get update && \\\n    apt-get install --no-install-recommends -y vim python-pip jq sudo curl && rm -rf /var/lib/apt/lists/*;\n\n# Install Bats\nRUN apt-get install --no-install-recommends -y software-properties-common && \\\n    add-apt-repository ppa:duggan/bats && \\\n    DEBIAN_FRONTEND=noninteractive apt-get update && \\\n    apt-get install --no-install-recommends -y bats && rm -rf /var/lib/apt/lists/*;\n\n# Install AWS CLI\nRUN pip install --no-cache-dir awscli --upgrade --user\n\n# Install moto: https://github.com/spulec/moto\nRUN pip install --no-cache-dir flask moto moto[server]\n\n# Install tools we'll need to create a mock EC2 metadata server\nRUN apt-get install --no-install-recommends -y net-tools iptables && rm -rf /var/lib/apt/lists/*;\n\n# Copy mock AWS CLI into the PATH\nCOPY ./.circleci/aws-local.sh /usr/local/bin/aws\n"
