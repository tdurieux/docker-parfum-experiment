  "repairedDockerfile": "FROM ubuntu:14.04\nMAINTAINER Peter McConnell <peter.mcconnell@rehabstudio.com>\n\n# keep upstart quiet\nRUN dpkg-divert --local --rename --add /sbin/initctl\nRUN ln -sf /bin/true /sbin/initctl\n\n# no tty\nENV DEBIAN_FRONTEND noninteractive\n\n# get up to date\nRUN apt-get update --fix-missing\n\n# global installs [applies to all envs!]\nRUN apt-get install --no-install-recommends -y build-essential git && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y python python-dev python-setuptools && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y python-pip python-virtualenv && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y nginx supervisor && rm -rf /var/lib/apt/lists/*;\n\n# stop supervisor service as we'll run it manually\nRUN service supervisor stop\n\n# build dependencies for postgres and image bindings\nRUN apt-get build-dep -y python-imaging python-psycopg2\n\n# create a virtual environment and install all depsendecies from pypi\nRUN virtualenv /opt/venv\nADD ./requirements.txt /opt/venv/requirements.txt\nRUN /opt/venv/bin/pip install -r /opt/venv/requirements.txt\n\n# expose port(s)\nEXPOSE 80\n"
