  "repairedDockerfile": "FROM ubuntu:16.04\n\nMAINTAINER Michael Chin\n\nRUN apt-get update -y\n\nRUN apt-get install --no-install-recommends -y git python-pip gcc && rm -rf /var/lib/apt/lists/*;\n\nRUN pip install --no-cache-dir -U setuptools\nRUN pip install --no-cache-dir -U pip\nRUN pip install --no-cache-dir jupyter\nRUN pip install --no-cache-dir netcdf4\nRUN pip install --no-cache-dir colorlover\n\nRUN apt-get install --no-install-recommends -y wget && rm -rf /var/lib/apt/lists/*;\n# install dependencies for pygplates\nRUN apt-get install --no-install-recommends -y libglew-dev && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y python2.7-dev && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y libboost-dev libboost-python-dev libboost-thread-dev \\\n    libboost-program-options-dev libboost-test-dev libboost-system-dev && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y libqt4-dev && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y libgdal-dev && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y libcgal-dev && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y libproj-dev && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y libqwt-dev && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y libxrender-dev libice-dev libsm-dev libfreetype6-dev libfontconfig1-dev && rm -rf /var/lib/apt/lists/*;\n\n# use wget to get the correct pygplates package from sourceforge\nRUN wget https://sourceforge.net/projects/gplates/files/pygplates/beta-revision-18/pygplates-ubuntu-xenial_2.1_1_amd64.deb\n\n# use dpkg to install\nRUN dpkg -i pygplates-ubuntu-xenial_2.1_1_amd64.deb\n\nRUN rm pygplates-ubuntu-xenial_2.1_1_amd64.deb\n\nEnv PYTHONPATH ${PYTHONPATH}:/usr/lib:/usr/lib/pygplates/revision18/\n\n# install all the python and ipython notebook requirements\nRUN apt-get install --no-install-recommends -y gcc python-pip && rm -rf /var/lib/apt/lists/*;\nRUN pip install --no-cache-dir --upgrade pip\nRUN pip install --no-cache-dir numpy scipy matplotlib jupyter pandas sympy nose\nRUN pip install --no-cache-dir ipyparallel pyproj==1.9.6 pyshp Pillow\nRUN pip install --no-cache-dir moviepy\n\nRUN wget https://github.com/matplotlib/basemap/archive/v1.2.0rel.tar.gz\nRUN tar -vxf v1.2.0rel.tar.gz && rm v1.2.0rel.tar.gz\nRUN cd basemap-1.2.0rel/ && python setup.py install_lib\nRUN rm -rf basemap-1.2.0rel/ v1.2.0rel.tar.gz\n\nRUN apt-get install --no-install-recommends -y gmt python-tk && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y cython && rm -rf /var/lib/apt/lists/*;\n\nRUN wget https://github.com/SciTools/cartopy/archive/v0.17.0.tar.gz\nRUN tar -vxf v0.17.0.tar.gz && rm v0.17.0.tar.gz\nRUN cd cartopy-0.17.0/ && python setup.py install\nRUN rm -rf cartopy-0.17.0/ v0.17.0.tar.gz\n\nRUN pip install --no-cache-dir statistics\n\nARG CACHE_DATE=2019-02-26\n\nRUN mkdir /home/workspace\nADD notebook.sh /home/workspace/\nRUN chmod a+x /home/workspace/notebook.sh\n\nCMD [\"/usr/local/bin/jupyter\", \"notebook\", \"--no-browser\", \"--allow-root\", \"--ip=0.0.0.0\", \"--NotebookApp.token=''\"]\n\n# Clean up APT when done.\nRUN apt-get clean && rm -rf /var/lib/apt/lists/* /tmp/* /var/tmp/*\n\nWORKDIR /home/workspace\n\nEXPOSE 8888\n"
}