  "repairedDockerfile": "FROM ubuntu:12.04\n\n# Allow custom PPA\nRUN echo \"deb http://archive.ubuntu.com/ubuntu precise universe\" >> /etc/apt/sources.list\nRUN apt-get install --no-install-recommends -y --force-yes software-properties-common python-software-properties && rm -rf /var/lib/apt/lists/*;\nRUN add-apt-repository -y ppa:chris-lea/node.js\nRUN apt-get update\n\n# Install NodeJS\nRUN apt-get install --no-install-recommends -y -q nodejs && rm -rf /var/lib/apt/lists/*;\n\n# Install gulp + coffeescript globally.\nRUN npm install -g coffee-script gulp && npm cache clean --force;\n\n# Install Ruby\nRUN apt-get install --no-install-recommends -y libgemplugin-ruby ruby && rm -rf /var/lib/apt/lists/*;\n# Install SASS\nRUN gem install sass\n\n# Remove unnecessary packages\nRUN apt-get autoremove -y\n\nADD run /code/client/run\nRUN chmod +x /code/client/run\nWORKDIR /code/client\nEXPOSE 35729\n\nCMD [\"/code/client/run\"]\n"
