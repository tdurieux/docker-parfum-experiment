  "repairedDockerfile": "# This image  minimal redis image based on https://hub.docker.com/r/nhoag/hubo &\n# https://github.com/pgarbe/tatsu-hubo\n\n# Pull base image.\nFROM rounds/10m-base\n\nMAINTAINER Aviv Laufer, aviv@rounds.com\nRUN apt-get update && \\\n    apt-get install --no-install-recommends -y software-properties-common && rm -rf /var/lib/apt/lists/*;\n\nRUN apt-get -y --no-install-recommends install expect nodejs npm python-pip && rm -rf /var/lib/apt/lists/*;\n\nRUN apt-get clean && \\\n    rm -rf /var/lib/apt/lists/*\n\nRUN ln -s /usr/bin/nodejs /usr/bin/node\n\nRUN npm install -g coffee-script && npm cache clean --force;\nRUN npm install -g yo generator-hubot && npm cache clean --force;\n\n# Create hubot user\nRUN useradd -d /hubot -m -s /bin/bash -U hubot\n#\n# # Log in as hubot user and change directory\nUSER hubot\nWORKDIR /hubot\n#\n# # Install hubot\nRUN yo hubot --owner=\"Golem <golem@rounds.com>\" --name=\"marvin\" --description=\"Marvin the Paranoid Android\" --defaults\n#\n# # Some adapters / scripts\nRUN npm install hubot-slack --save && npm install && npm cache clean --force;\nRUN npm install hubot-standup-alarm --save && npm install && npm cache clean --force;\nRUN npm install hubot-auth --save && npm install && npm cache clean --force;\nRUN npm install hubot-google-translate --save && npm install && npm cache clean --force;\nRUN npm install hubot-alias --save && npm install && npm cache clean --force;\nRUN npm install hubot-youtube --save && npm install && npm cache clean --force;\n\nRUN npm install hubot-diagnostics  --save && npm install && npm cache clean --force;\nRUN npm install hubot-google-images  --save && npm install && npm cache clean --force;\nRUN npm install hubot-help  --save && npm install && npm cache clean --force;\nRUN npm install hubot-pugme  --save && npm install && npm cache clean --force;\nRUN npm install hubot-redis-brain  --save && npm install && npm cache clean --force;\nRUN npm install hubot-rules  --save && npm install && npm cache clean --force;\nRUN npm install hubot-scripts  --save && npm install && npm cache clean --force;\nRUN npm install hubot-shipit  --save && npm install && npm cache clean --force;\nRUN npm install hubot-devops-reactions  --save && npm install && npm cache clean --force;\nRUN npm install hubot-has-no-idea  --save && npm install && npm cache clean --force;\nRUN npm install moment  --save && npm install && npm cache clean --force;\nRUN npm install htmlparser  --save && npm install && npm cache clean --force;\nRUN npm install soupselect  --save && npm install && npm cache clean --force;\nRUN npm install xml2js --save && npm install && npm cache clean --force;\nRUN npm install hubot-seen --save && npm install && npm cache clean --force;\n#\n# # Activate some built-in scripts\nADD hubot/hubot-scripts.json /hubot/\nADD hubot/external-scripts.json /hubot/\n#\nRUN npm install cheerio --save && npm install && npm cache clean --force;\nADD hubot/scripts/* /hubot/scripts/\n#\n# # And go\n#CMD [\"/bin/sh\", \"-c\", \"aws s3 cp --region eu-west-1 s3://pgarbe-secrets/env.sh .; . ./env.sh; bin/hubot --adapter slack\"]\n#:\nRUN sed -e'/^export/a export REDIS_URL=\"${REDIS_PORT}\"' bin/hubot > bin/hubot.run\nRUN chmod +x bin/hubot.run\nCMD bin/hubot.run --adapter slack\n"
