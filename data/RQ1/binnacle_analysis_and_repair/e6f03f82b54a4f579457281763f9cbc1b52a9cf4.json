  "repairedDockerfile": "FROM ubuntu:12.04\n\n# hopefully temporary work-around of http://git.io/Ke_Meg#1724\nRUN apt-mark hold initscripts udev plymouth mountall\n\n# Add package sources\nRUN apt-get -y update\nRUN apt-get -y --no-install-recommends install python-software-properties && rm -rf /var/lib/apt/lists/*;\nRUN add-apt-repository ppa:ubuntu-toolchain-r/test\n\n\nRUN apt-get update\nRUN apt-get -y upgrade\n\n\n# Various dependencies\nRUN apt-get -y --no-install-recommends install pkg-config && rm -rf /var/lib/apt/lists/*;\nRUN apt-get -y --no-install-recommends install git && rm -rf /var/lib/apt/lists/*;\nRUN apt-get -y --no-install-recommends install scons && rm -rf /var/lib/apt/lists/*;\nRUN apt-get -y --no-install-recommends install ctags && rm -rf /var/lib/apt/lists/*;\nRUN add-apt-repository ppa:boost-latest/ppa && apt-get update && apt-get -y --no-install-recommends install libboost1.55-all-dev && rm -rf /var/lib/apt/lists/*;\nRUN apt-get -y --no-install-recommends install protobuf-compiler && rm -rf /var/lib/apt/lists/*;\nRUN apt-get -y --no-install-recommends install libprotobuf-dev && rm -rf /var/lib/apt/lists/*;\nRUN apt-get -y --no-install-recommends install libssl-dev && rm -rf /var/lib/apt/lists/*;\n\n# Upgrade to gcc-4.7 (after some other package pulls in gcc)\nRUN apt-get -y --no-install-recommends install gcc-4.8 g++-4.8 && rm -rf /var/lib/apt/lists/*;\nRUN rm -f /usr/bin/gcc && ln -s /usr/bin/gcc-4.8 /usr/bin/gcc\nRUN rm -f /usr/bin/g++ && ln -s /usr/bin/g++-4.8 /usr/bin/g++\n\n\n# Checkout the ripple source\nRUN git clone https://github.com/ripple/rippled.git /opt/rippled -b release\nRUN cd /opt/rippled && scons build/rippled\n\n\n# peer_port\nEXPOSE 51235\n# websocket_public_port\nEXPOSE 5006\n# websocket_port (trusted access)\nEXPOSE 6006\n\n\n# Share the ripple data directory\nVOLUME /var/lib/rippled\n\n# Add custom config\nADD rippled.conf /opt/rippled/build/rippled.conf\n\nCMD [\"/opt/rippled/build/rippled\", \"--conf\", \"/opt/rippled/build/rippled.conf\"]\n\n\n"
