  "repairedDockerfile": "FROM debian:jessie\nENV DEBIAN_FRONTEND noninteractive\nRUN apt-get update && apt-get -y upgrade && apt-get install --no-install-recommends -y apt-utils curl vim && rm -rf /var/lib/apt/lists/*;\n\n# nodejs, npm\nRUN curl -f -sL https://deb.nodesource.com/setup_5.x | bash -\nRUN apt-get install --no-install-recommends -y nodejs && rm -rf /var/lib/apt/lists/*;\n\n# git\nRUN apt-get install --no-install-recommends -y git && rm -rf /var/lib/apt/lists/*;\n\n# source\nRUN mkdir /hackmd\nWORKDIR /hackmd\nRUN git clone https://github.com/hackmdio/hackmd.git /hackmd\nRUN git checkout 018beab583b5affb59321762388e376d5f123b0f\n\n# npm, deps\nRUN apt-get install --no-install-recommends -y python build-essential make gcc g++ libkrb5-dev && rm -rf /var/lib/apt/lists/*;\nRUN npm install && npm cache clean --force;\n\n# bower\nRUN npm install -g bower && npm cache clean --force;\nRUN bower install --allow-root\n\n# add config\nADD config.js /hackmd/config.js\nADD docker-entrypoint.sh /hackmd/docker-entrypoint.sh\n\n# psql client\nRUN apt-get install --no-install-recommends -y postgresql-client-9.4 && rm -rf /var/lib/apt/lists/*;\n\nEXPOSE 3000\n\nCMD [\"/bin/bash\", \"/hackmd/docker-entrypoint.sh\"]\n\n"
