  "repairedDockerfile": "FROM ubuntu:trusty\nENV DEBIAN_FRONTEND noninteractive\n\n# nginx\nRUN apt-get update -q\nRUN apt-get install --no-install-recommends -yf build-essential python-software-properties software-properties-common && rm -rf /var/lib/apt/lists/*;\nRUN add-apt-repository ppa:nginx/stable\nRUN apt-get update -q\nRUN apt-get -y --no-install-recommends install -y curl && rm -rf /var/lib/apt/lists/*;\n\n# build nginx from source with http auth module enabled\nRUN apt-get -y --no-install-recommends install libpcre3-dev zlib1g-dev libssl-dev && rm -rf /var/lib/apt/lists/*;\nRUN curl -f -O http://nginx.org/download/nginx-1.6.1.tar.gz\nRUN tar -xzf nginx-1.6.1.tar.gz && rm nginx-1.6.1.tar.gz\nWORKDIR nginx-1.6.1\nRUN ./configure --build=\"$(dpkg-architecture --query DEB_BUILD_GNU_TYPE)\" --with-http_ssl_module --with-http_auth_request_module && make && make install\n\n# install pystache\nRUN apt-get -y --no-install-recommends install python-pip && rm -rf /var/lib/apt/lists/*;\nRUN pip install --no-cache-dir pystache\n\n# nginx configuration\nADD nginx/nginx.conf /usr/local/nginx/conf/nginx.conf\nADD nginx/nginx.default /usr/local/nginx/conf/sites-enabled/default.template\nADD start.sh /start.sh\n\nEXPOSE 80\nCMD /start.sh\n"
