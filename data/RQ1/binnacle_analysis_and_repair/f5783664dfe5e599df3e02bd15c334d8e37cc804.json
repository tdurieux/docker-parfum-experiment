  "repairedDockerfile": "FROM ubuntu:16.04\nMAINTAINER Tim O'Shea <tim.oshea753@gmail.com>\n\n# set up environment\nENV DEBIAN_FRONTEND noninteractive\n\n# update repos/ppas...\nRUN apt-get update\nRUN apt-get install --no-install-recommends -y python-software-properties software-properties-common && rm -rf /var/lib/apt/lists/*;\nRUN apt-add-repository -y ppa:x2go/stable\nRUN apt-get update\n\n# install core packages\nRUN apt-get install --no-install-recommends -y python-pip git openssh-server vim emacs screen tmux locate && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y python-matplotlib python-scipy python-numpy && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y python-numpy python-dev && rm -rf /var/lib/apt/lists/*;\n\n# Set up remove login info\nRUN mkdir /var/run/sshd\nRUN echo 'root:radioml' | chpasswd\nRUN sed -i 's/PermitRootLogin without-password/PermitRootLogin yes/' /etc/ssh/sshd_config\n\n# somewhat more graphical packages..\nRUN apt-get install --no-install-recommends -y firefox evince audacity meld && rm -rf /var/lib/apt/lists/*;\n\n# set up remove visual login packages ...\nRUN apt-get install --no-install-recommends -y xfwm4 xfce4 x2goserver x2goserver-xsession && rm -rf /var/lib/apt/lists/*;\n\n# install python packages\nRUN pip install --no-cache-dir --upgrade pip\n\n# set up gnuradio and related tools\nRUN apt-get install --no-install-recommends -y autotools-dev autoconf sudo wireshark gdb && rm -rf /var/lib/apt/lists/*;\nRUN pip install --no-cache-dir --upgrade git+https://github.com/gnuradio/pybombs.git\nRUN mkdir /gr/\nRUN cd /gr/ && pybombs prefix init .\nRUN cd /gr/ && pybombs recipes add gr-recipes git+https://github.com/gnuradio/gr-recipes.git\nRUN cd /gr/ && pybombs recipes add gr-etcetera git+https://github.com/gnuradio/gr-etcetera.git\nRUN cd /gr/ && pybombs install gnuradio gr-burst gr-pyqt gr-pcap gr-mapper gr-analysis gr-mediatools\n\n# check out sources for reference\nRUN /bin/ln -s /gr/src/ /root/src\n\n# copy in some helpful files / set up env on login\nCOPY .vimrc /root/\nRUN echo \"source /gr/setup_env.sh\" >> /root/.bashrc\n\n\n\n"
