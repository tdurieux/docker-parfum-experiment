  "repairedDockerfile": "FROM ubuntu:latest \n\n#Autor \nMAINTAINER √ÅNGEL VALERA MOTOS <angelvalera.epal@gmail.com> \n\n#Actualizar Sistema Base \nRUN sudo apt-get -y update \n\n#Descargar aplicacion \nRUN sudo apt-get install --no-install-recommends -y git && rm -rf /var/lib/apt/lists/*;\nRUN sudo git clone https://github.com/AngelValera/proyectoIV-Modulo-1.git \n\n# Instalar Python y PostgreSQL \nRUN sudo apt-get install --no-install-recommends -y python-setuptools && rm -rf /var/lib/apt/lists/*;\nRUN sudo apt-get -y --no-install-recommends install python-dev && rm -rf /var/lib/apt/lists/*;\nRUN sudo apt-get -y --no-install-recommends install build-essential && rm -rf /var/lib/apt/lists/*;\nRUN sudo apt-get -y --no-install-recommends install python-psycopg2 && rm -rf /var/lib/apt/lists/*;\nRUN sudo apt-get -y --no-install-recommends install libpq-dev && rm -rf /var/lib/apt/lists/*;\nRUN sudo easy_install pip \nRUN sudo pip install --upgrade pip \n\n#Instalar la app \nRUN ls \nRUN cd proyectoIV-Modulo-1/ && ls -l \nRUN cd proyectoIV-Modulo-1/ && cat requirements.txt \nRUN cd proyectoIV-Modulo-1/ && sudo pip install --no-cache-dir -r requirements.txt\n\n#Migraciones \nRUN cd proyectoIV-Modulo-1/ && python manage.py migrate --noinput \n\n"
