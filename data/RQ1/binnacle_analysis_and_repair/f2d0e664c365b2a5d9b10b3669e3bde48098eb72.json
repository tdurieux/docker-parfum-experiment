  "repairedDockerfile": "FROM smartapps/bitbucket-pipelines-php-mysql \nMAINTAINER Nemanja Ognjanovic <nemanja.ognjanovic@ringieraxelspringer.rs> \nRUN apt-get update \nRUN apt-get install --no-install-recommends --yes php5-fpm && rm -rf /var/lib/apt/lists/*;\nRUN service php5-fpm start \nRUN apt-get install --no-install-recommends --yes memcached && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends --yes php-pear && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends --yes redis-server && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends --yes php5-dev && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends --yes php5-apcu && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends --yes php5-tidy && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends --yes libicu-dev && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends --yes php5-intl && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends --yes php5-igbinary && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends --yes php5-memcache && rm -rf /var/lib/apt/lists/*;\nRUN service memcached start \nRUN apt-get install --no-install-recommends --yes php5-xdebug && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends --yes php5-redis && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends --yes php5-dev && rm -rf /var/lib/apt/lists/*;\nRUN service redis-server start \nRUN service mysql start \nRUN git clone -q --depth=1 https://github.com/phalcon/cphalcon.git -b 2.0.x \nWORKDIR /cphalcon/ext \nRUN export CFLAGS=\"-g3 -O1 -fno-delete-null-pointer-checks -Wall\"; \nRUN phpize \nRUN ./configure --build=\"$(dpkg-architecture --query DEB_BUILD_GNU_TYPE)\" --enable-phalcon\nRUN make -j4 \nRUN make install \nWORKDIR / \nRUN rm -rf /cphalcon \nRUN touch /etc/php5/cli/conf.d/10-phalcon.ini \nRUN echo \"extension=phalcon.so\" >> /etc/php5/cli/conf.d/10-phalcon.ini \nRUN touch /etc/php5/fpm/conf.d/10-phalcon.ini \nRUN echo \"extension=phalcon.so\" > /etc/php5/fpm/conf.d/10-phalcon.ini \nEXPOSE 9000\n\n"
