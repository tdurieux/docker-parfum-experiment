  "repairedDockerfile": "# Create image\n# docker build -t git-lfs-server .\n\n# Create container\n# docker run -it --name git-lfs-server -u test -v `pwd`:/home/test/git-lfs-server git-lfs-server\n\nFROM ubuntu:trusty\n\nMAINTAINER Stanislav Artemkin\n\nARG DEBIAN_FRONTEND=noninteractive\n\nRUN apt-get update\nRUN apt-get install -y --no-install-recommends -qq software-properties-common && rm -rf /var/lib/apt/lists/*;\n\nRUN echo \"yes\" | add-apt-repository ppa:avsm/ppa\nRUN apt-get update\n\nRUN apt-get install -y --no-install-recommends -qq apt-utils && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install -y --no-install-recommends -qq man && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install -y --no-install-recommends -qq sudo && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install -y --no-install-recommends -qq m4 pkg-config libssl-dev libgmp-dev && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install -y --no-install-recommends -qq curl && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install -y --no-install-recommends -qq make && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install -y --no-install-recommends -qq git && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install -y --no-install-recommends -qq g++ && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install -y --no-install-recommends -qq unzip && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install -y --no-install-recommends -qq ocaml ocaml-native-compilers camlp4-extra opam && rm -rf /var/lib/apt/lists/*;\n\n# pam-devel on CentOS\nRUN apt-get install -y --no-install-recommends -qq libpam0g-dev && rm -rf /var/lib/apt/lists/*;\n\n# Create user\nRUN adduser test --gecos \"First Last,RoomNumber,WorkPhone,HomePhone\" --disabled-password\nRUN echo \"test:test\" | chpasswd\nRUN echo \"test    ALL=(ALL:ALL) ALL\" >> /etc/sudoers\n\nRUN su - test -c 'echo export OPAMYES=1 >> ~/.profile'\nRUN su - test -c 'opam init'\nRUN su - test -c 'opam switch 4.04.2'\nRUN su - test -c 'opam update'\nRUN su - test -c 'opam install async async_ssl cohttp cohttp-async cryptokit yojson simple_pam utop'\nRUN su - test -c 'opam install bisect ocveralls ocamldiff'\n\n"
