  "repairedDockerfile": "FROM ubuntu:14.04\n\nENV DEBIAN_FRONTEND noninteractive\n\nRUN apt-get -qq update\n\nRUN apt-get -qq --no-install-recommends install -qq -y software-properties-common python-software-properties curl && rm -rf /var/lib/apt/lists/*;\nRUN add-apt-repository ppa:chris-lea/redis-server\nRUN apt-get -qq update\nRUN apt-get -qq --no-install-recommends install -qq -y beanstalkd && rm -rf /var/lib/apt/lists/*;\nRUN apt-get -qq --no-install-recommends install -qq -y redis-server && rm -rf /var/lib/apt/lists/*;\nRUN apt-get -qq --no-install-recommends install -qq -y mysql-server && rm -rf /var/lib/apt/lists/*;\nRUN apt-get -qq --no-install-recommends install -qq -y postgresql postgresql-contrib && rm -rf /var/lib/apt/lists/*;\nRUN apt-get -qq --no-install-recommends install -qq -y make && rm -rf /var/lib/apt/lists/*;\nRUN apt-get -qq --no-install-recommends install -qq -y git && rm -rf /var/lib/apt/lists/*;\n\n# `language-pack-en-base` is necessary to properly install the key for ppa:ondrej/php5-5.6\nRUN \\\n    locale-gen en_US.UTF-8 && \\\n    apt-get -qq --no-install-recommends install -qq -y language-pack-en-base && rm -rf /var/lib/apt/lists/*;\nRUN LC_ALL=en_US.UTF-8 add-apt-repository ppa:ondrej/php && \\\n    apt-get -qq update\nRUN apt-get -qq --no-install-recommends install -qq -y php5.6-cli php5.6-curl php5.6-mysql php5.6-pgsql php-pear php5.6-xdebug php5.6-redis php5.6-dom php5.6-zip && rm -rf /var/lib/apt/lists/*;\n\nRUN \\\n    curl -f -sS https://getcomposer.org/installer | php && \\\n    mv composer.phar /usr/local/bin/composer && \\\n    chmod +x /usr/local/bin/composer && \\\n    composer global require phpunit/phpunit ^5.0 && \\\n    ln -s ~/.composer/vendor/bin/phpunit /usr/local/bin/phpunit && \\\n    phpunit --version\n\nADD composer.json /app/composer.json\nWORKDIR /app\n\nRUN \\\n    composer install > /dev/null && \\\n    pear install --alldeps PHP_CodeSniffer > /dev/null\n\nENV BEANSTALK_URL=\"beanstalk://127.0.0.1:11300?queue=default&timeout=1\" \\\n    MEMORY_URL=\"memory:///?queue=default&timeout=1\" \\\n    MYSQL_URL=\"mysql://travis@127.0.0.1:3306/database_name?queue=default&timeout=1\" \\\n    NULL_URL=\"null:///?queue=default&timeout=1\" \\\n    REDIS_URL=\"redis://127.0.0.1:6379/0?queue=default&timeout=1\" \\\n    MEMORY_URL=\"synchronous:///?queue=default&timeout=1\" \\\n    POSTGRES_URL=\"pgsql://travis:asdf12@127.0.0.1:5432/database_name?queue=default\"\n\nADD . /app\n\nRUN make test-docker\n"
