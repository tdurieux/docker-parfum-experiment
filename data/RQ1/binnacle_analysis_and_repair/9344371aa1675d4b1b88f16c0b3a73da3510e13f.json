  "repairedDockerfile": "############################################################\n# Dockerfile to build subread-1.5.0-p1 container images\n# Based on Ubuntu\n############################################################\n\n# Set the base image to Ubuntu\nFROM ubuntu:14.04\n\n# File Author / Maintainer\nMAINTAINER Sophie Lemoine\n\n# Update the repository sources list\nRUN apt-get update\n\n# Install compiler\nRUN apt-get install --no-install-recommends --yes \\\n build-essential \\\n gcc-multilib \\\n apt-utils \\\n zlib1g-dev && rm -rf /var/lib/apt/lists/*;\n\n# Install Latex\nRUN apt-get install --no-install-recommends --yes texlive-latex-base texlive-latex-extra && rm -rf /var/lib/apt/lists/*;\n\n# Install libxml2\nRUN apt-get install --no-install-recommends --yes libxml2-dev && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends --yes \\\n wget && rm -rf /var/lib/apt/lists/*;\n\n# Install subread\nWORKDIR /usr/local/\nRUN wget https://heanet.dl.sourceforge.net/project/subread/subread-1.5.0-p1/subread-1.5.0-p1-source.tar.gz\nRUN tar xzf subread-1.5.0-p1-source.tar.gz && rm subread-1.5.0-p1-source.tar.gz\nWORKDIR /usr/local/subread-1.5.0-p1-source/src\nRUN make -f Makefile.Linux\nRUN ln -s /usr/local/subread-1.5.0-p1-source/bin/* /usr/local/bin\n\n\n# Cleanup\nRUN apt-get clean\n\n# Default command to execute at startup of the container\nCMD R --no-save\n\n# Install vim\nRUN apt-get install --no-install-recommends --yes vim && rm -rf /var/lib/apt/lists/*;\n"
