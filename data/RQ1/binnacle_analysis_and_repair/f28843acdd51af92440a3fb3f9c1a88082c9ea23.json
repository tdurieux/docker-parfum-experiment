  "repairedDockerfile": "FROM ubuntu:latest\nMAINTAINER Ori Pekelman <ori@pekelman.com>\n# We need mongodb\n# Add 10gen official apt source to the sources list\nRUN apt-key adv --keyserver hkp://keyserver.ubuntu.com:80 --recv 7F0CEB10\nRUN echo 'deb http://downloads-distro.mongodb.org/repo/ubuntu-upstart dist 10gen' | tee /etc/apt/sources.list.d/mongodb.list\n\n# Hack for initctl not being available in Ubuntu\nRUN dpkg-divert --local --rename --add /sbin/initctl\nRUN ln -s /bin/true /sbin/initctl\n# Install MongoDB\nRUN apt-get update\n\n#We ran into some weird shit starting mongo so this is the dance around locales\nRUN apt-get -y --no-install-recommends install language-pack-en-base && rm -rf /var/lib/apt/lists/*;\nRUN dpkg-reconfigure locales\nRUN echo \"export LC_ALL=C\">> /etc/environment\nRUN apt-get install -y --no-install-recommends mongodb-10gen && rm -rf /var/lib/apt/lists/*;\n# Create the MongoDB data directory\nRUN mkdir -p /data/db\n#We need hadoop it, has dependencies\n#RUN apt-get install -y rsync ssh\nRUN apt-get install --no-install-recommends -y python-software-properties software-properties-common && rm -rf /var/lib/apt/lists/*;\nRUN add-apt-repository ppa:hadoop-ubuntu/stable\nRUN apt-get update\nRUN apt-get -y --no-install-recommends install hadoop && rm -rf /var/lib/apt/lists/*;\n\n#We want to be able to ssh in\nRUN apt-get install --no-install-recommends -y openssh-server && rm -rf /var/lib/apt/lists/*;\nRUN mkdir /var/run/sshd\nRUN mkdir ~/.ssh/\nRUN ssh-keygen -t dsa -P '' -f ~/.ssh/id_dsa\nRUN cat ~/.ssh/id_dsa.pub >> ~/.ssh/authorized_keys\nRUN echo 'root:changeme' |chpasswd\n\nADD PredictionIO-0.6.3.zip /tmp/PredictionIO-0.6.3.zip\nRUN apt-get install -y --no-install-recommends unzip && rm -rf /var/lib/apt/lists/*;\nRUN cd /tmp && unzip PredictionIO-0.6.3.zip\nRUN rm /tmp/PredictionIO-0.6.3.zip\nRUN mv /tmp/PredictionIO-0.6.3 /var/lib/predictionio\nRUN apt-get -y --no-install-recommends install curl && rm -rf /var/lib/apt/lists/*;\nRUN /usr/bin/mongod --config /etc/mongodb.conf --fork && sleep 1 && /var/lib/predictionio/bin/setup.sh\n\n\nRUN apt-get install --no-install-recommends -y python-setuptools && rm -rf /var/lib/apt/lists/*;\nRUN easy_install supervisor\nRUN mkdir -p /var/log/supervisor\nADD supervisord.conf /usr/local/etc/supervisord.conf\n\n\nEXPOSE 27017 22 8000\nCMD  [\"/usr/local/bin/supervisord\", \"-n\"]\n"
