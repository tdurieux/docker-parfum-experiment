  "repairedDockerfile": "FROM ubuntu:14.04\nMAINTAINER Falcon wuzhangjin@gmail.com\n\n# mirrors.163.com\nRUN sed -i -e \"s/archive.ubuntu.com/123.58.173.185/g\" /etc/apt/sources.list\n\nRUN apt-get -y update\n\nRUN apt-get install --no-install-recommends -y git vim gcc g++ && rm -rf /var/lib/apt/lists/*;\n\nRUN apt-get install --no-install-recommends -y gcc-arm-linux-gnueabihf && rm -rf /var/lib/apt/lists/*;\n\n# For Qemu beagle board support: qemu-linaro comiple\nRUN apt-get install --no-install-recommends -y qemu-common qemu-keymaps && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y libglib2.0-dev zlib1g-dev libpixman-1-dev libfdt-dev libtool && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y make u-boot-tools && rm -rf /var/lib/apt/lists/*;\n# RUN git clone git://git.linaro.org/qemu/qemu-linaro.git\nRUN git clone https://github.com/choonho/qemu-beagle.git\n# ADD qemu-beagle /qemu-beagle/\nWORKDIR qemu-beagle/\nRUN ./configure --build=\"$(dpkg-architecture --query DEB_BUILD_GNU_TYPE)\" --target-list=arm-softmmu --disable-strip --enable-debug\nRUN make -j8 && make install\n\nRUN mkdir /hot-pot-lab/\nWORKDIR /hot-pot-lab/\n\nENTRYPOINT [\"/bin/bash\"]\n"
