  "repairedDockerfile": "FROM ubuntu:17.04\nRUN apt-get update\nRUN apt-get install --no-install-recommends -y apt-utils && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y python3-pip && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y libzmq3-dev && rm -rf /var/lib/apt/lists/*;\nRUN pip3 install --no-cache-dir jupyter\nENV PATH /root/.local/bin:$PATH\n\nRUN apt-get install --no-install-recommends -y z3=4.4.1\\* && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y ruby && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y smlnj libsmlnj-smlnj ml-yacc ml-ulex && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y mlton && rm -rf /var/lib/apt/lists/*;\n\nENV TIML /timl\n\nWORKDIR $TIML\n\nADD . $TIML\nENV PATH $PATH:$TIML\nENV PYTHONPATH $PYTHONPATH:$TIML/jupyter\n\nRUN mkdir -p /root/.local/share/jupyter/kernels/timl\nRUN ln -s $TIML/jupyter/timl-kernel.json /root/.local/share/jupyter/kernels/timl/kernel.json\nRUN jupyter kernelspec install ~/.local/share/jupyter/kernels/timl\n\nRUN mkdir /notebooks\nRUN cp $TIML/jupyter/TiML-example-notebook.ipynb /notebooks\nRUN cp -r $TIML/examples /notebooks\nENTRYPOINT jupyter notebook --allow-root --NotebookApp.port=8888 '--NotebookApp.ip=*' --NotebookApp.notebook_dir=/notebooks --NotebookApp.token=''\nEXPOSE 8888\n"
