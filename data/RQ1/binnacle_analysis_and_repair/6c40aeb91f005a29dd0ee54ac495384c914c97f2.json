  "repairedDockerfile": "FROM nvidia/cuda:7.5-cudnn4-devel\nMAINTAINER Tim O'Shea <tim.oshea753@gmail.com>\n\n# set up environment\nENV DEBIAN_FRONTEND noninteractive\n\n# update repos/ppas...\nRUN apt-get update\nRUN apt-get install --no-install-recommends -y python-software-properties software-properties-common && rm -rf /var/lib/apt/lists/*;\nRUN apt-add-repository -y ppa:x2go/stable\nRUN apt-get update\n\n# install core packages\nRUN apt-get install --no-install-recommends -y python-pip git openssh-server vim emacs screen tmux locate && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y python-matplotlib python-scipy python-numpy && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y python-sklearn python-sklearn-doc python-skimage python-skimage-doc python-scikits-learn python-scikits.statsmodels && rm -rf /var/lib/apt/lists/*;\n\n# Set up remove login info\nRUN mkdir /var/run/sshd\nRUN echo 'root:radioml' | chpasswd\nRUN sed -i 's/PermitRootLogin without-password/PermitRootLogin yes/' /etc/ssh/sshd_config\n\n# somewhat more graphical packages..\nRUN apt-get install --no-install-recommends -y python-opencv gimp && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y firefox evince audacity meld && rm -rf /var/lib/apt/lists/*;\n\n# set up remove visual login packages ...\nRUN apt-get install --no-install-recommends -y xfwm4 xfce4 x2goserver x2goserver-xsession && rm -rf /var/lib/apt/lists/*;\n\n# install python packages\nRUN pip install --no-cache-dir --upgrade pip\nRUN pip install --no-cache-dir --upgrade ipython[all]\nRUN pip install --no-cache-dir --upgrade --no-deps git+git://github.com/Theano/Theano.git\nRUN pip install --no-cache-dir --upgrade https://storage.googleapis.com/tensorflow/linux/cpu/tensorflow-0.9.0-cp27-none-linux_x86_64.whl\nRUN pip install --no-cache-dir --upgrade git+https://github.com/fchollet/keras.git\nRUN pip install --no-cache-dir --upgrade seaborn tqdm\nRUN pip install --no-cache-dir --upgrade pandas\n\n# set up gnuradio and related tools\nRUN apt-get install --no-install-recommends -y sudo && rm -rf /var/lib/apt/lists/*;\n\n# check out sources for reference\nRUN mkdir /root/src/\nRUN cd /root/src/ && git clone https://github.com/Theano/Theano.git\nRUN cd /root/src/ && git clone https://github.com/tensorflow/tensorflow.git\nRUN cd /root/src/ && git clone https://github.com/fchollet/keras.git\n\n# Build PyOpenPNL and Gym deps\nRUN pip install --no-cache-dir networkx\nRUN apt-get install --no-install-recommends -y python-numpy python-dev cmake zlib1g-dev libjpeg-dev xvfb libav-tools xorg-dev python-opengl libboost-all-dev libsdl2-dev swig pypy-dev automake autoconf libtool && rm -rf /var/lib/apt/lists/*;\nRUN cd /root/src/ && git clone https://github.com/PyOpenPNL/OpenPNL.git && cd OpenPNL && ./autogen.sh && ./configure --build=\"$(dpkg-architecture --query DEB_BUILD_GNU_TYPE)\" CFLAGS='-g -O2 -fpermissive -w' CXXFLAGS='-g -O2 -fpermissive -w' && make -j4 && make install\nRUN cd /root/src/ && git clone https://github.com/PyOpenPNL/PyOpenPNL.git && cd PyOpenPNL && python setup.py build && python setup.py install\nRUN cd /root/src/ && git clone https://github.com/osh/kerlym.git && cd kerlym && python setup.py build && python setup.py install\n\n# set up OpenAI Gym\nRUN cd /root/src/ && git clone https://github.com/openai/gym.git && cd gym && pip install --no-cache-dir -e .\nRUN pip install --no-cache-dir gym[atari] pachi_py\nRUN mkdir /root/src/notebooks/\n\n# copy in some helpful files / set up env on login\nCOPY .vimrc /root/\n\n\n\n"
