  "repairedDockerfile": "FROM ubuntu:latest \n# MAINTAINER Hyon S Chu <hyon.s.chu@accenture.com> \nCMD echo Installing Accenture Tech Labs Scientific Python Enviro \n\nRUN apt-get install --no-install-recommends python -y && rm -rf /var/lib/apt/lists/*;\nRUN apt-get update && apt-get upgrade -y \nRUN apt-get install --no-install-recommends curl -y && rm -rf /var/lib/apt/lists/*;\nRUN curl -f -O https://bootstrap.pypa.io/get-pip.py\nRUN python get-pip.py \nRUN rm get-pip.py \nRUN echo \"export PATH=~/.local/bin:$PATH\" >> ~/.bashrc \nRUN apt-get install --no-install-recommends python-setuptools build-essential python-dev -y && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends gfortran swig -y && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends libatlas-dev liblapack-dev -y && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends libfreetype6 libfreetype6-dev -y && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends libxft-dev -y && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install -y --no-install-recommends libxml2-dev libxslt-dev zlib1g-dev && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install -y --no-install-recommends python-numpy && rm -rf /var/lib/apt/lists/*;\n\nADD requirements.txt /tmp/requirements.txt \nRUN pip install --no-cache-dir -r /tmp/requirements.txt -q\n\n"
