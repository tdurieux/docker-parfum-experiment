  "repairedDockerfile": "FROM php:7.1.20-apache\n\nRUN apt-get -y update --fix-missing\nRUN apt-get upgrade -y\n\n# Install useful tools\nRUN apt-get -y --no-install-recommends install apt-utils nano wget dialog && rm -rf /var/lib/apt/lists/*;\n\n# Install important libraries\nRUN apt-get -y --no-install-recommends install --fix-missing apt-utils build-essential git curl libcurl3 libcurl3-dev zip && rm -rf /var/lib/apt/lists/*;\n\n# Composer\nRUN curl -f -sS https://getcomposer.org/installer | php -- --install-dir=/usr/local/bin --filename=composer\n\n# Install xdebug\nRUN pecl install xdebug-2.5.0\nRUN docker-php-ext-enable xdebug\n\n# Other PHP7 Extensions\n\nRUN apt-get -y --no-install-recommends install libmcrypt-dev && rm -rf /var/lib/apt/lists/*;\nRUN docker-php-ext-install mcrypt\n\nRUN apt-get -y --no-install-recommends install libsqlite3-dev libsqlite3-0 mysql-client && rm -rf /var/lib/apt/lists/*;\nRUN docker-php-ext-install pdo_mysql\nRUN docker-php-ext-install pdo_sqlite\nRUN docker-php-ext-install mysqli\n\nRUN docker-php-ext-install curl\nRUN docker-php-ext-install tokenizer\nRUN docker-php-ext-install json\n\nRUN apt-get -y --no-install-recommends install zlib1g-dev && rm -rf /var/lib/apt/lists/*;\nRUN docker-php-ext-install zip\n\nRUN apt-get -y --no-install-recommends install libicu-dev && rm -rf /var/lib/apt/lists/*;\nRUN docker-php-ext-install -j$(nproc) intl\n\nRUN docker-php-ext-install mbstring\n\nRUN apt-get install --no-install-recommends -y libfreetype6-dev libjpeg62-turbo-dev libpng-dev && rm -rf /var/lib/apt/lists/*;\nRUN docker-php-ext-configure gd --with-freetype-dir=/usr/include/ --with-jpeg-dir=/usr/include/\nRUN docker-php-ext-install -j$(nproc) gd\n\n# Enable apache modules\nRUN a2enmod rewrite headers\n"
