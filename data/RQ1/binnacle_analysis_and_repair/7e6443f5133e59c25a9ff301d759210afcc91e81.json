  "repairedDockerfile": "FROM ubuntu:12.04\n\nRUN apt-get update\n\n# Additional tooling\nRUN apt-get install --no-install-recommends -y git && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y curl && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y mercurial && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y subversion && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y unzip && rm -rf /var/lib/apt/lists/*;\n\n# Build tools\nRUN apt-get install --no-install-recommends -y build-essential && rm -rf /var/lib/apt/lists/*;\n\n# Install Ruby from Brightbox's PPA\nRUN apt-get -y --no-install-recommends install python-software-properties && rm -rf /var/lib/apt/lists/*;\nRUN apt-add-repository ppa:brightbox/ruby-ng\nRUN apt-get update\nRUN apt-get -y --no-install-recommends install ruby2.1 && rm -rf /var/lib/apt/lists/*;\nRUN apt-get -y --no-install-recommends install ruby2.1-dev && rm -rf /var/lib/apt/lists/*;\n\n# Update rubygems and don't install docs\nRUN mkdir -p /usr/local/etc \\\n  && { \\\n    echo 'install: --no-document'; \\\n    echo 'update: --no-document'; \\\n  } >> /usr/local/etc/gemrc;\nRUN gem update --system --verbose && rm -rf /root/.gem;\nRUN echo \"gem: --no-document\" >> ~/.gemrc\n\n# Install fpm-cookery\nRUN gem install fpm-cookery -v 0.31.0\n\n# Remove strict host key checking for SSH\nRUN echo 'StrictHostKeyChecking no' >> /etc/ssh/ssh_config\n\n# Set an entry point to simplify command execution\nENTRYPOINT [\"/usr/local/bin/fpm-cook\"]\n"
