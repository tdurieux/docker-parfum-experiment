  "repairedDockerfile": "FROM ubuntu:15.04\nMAINTAINER Peter Belmann\n\n#add data\nRUN mkdir /home/biogas\nADD Makefile /home/biogas/Makefile\nADD run.sh /home/biogas/run.sh\n\n#install ray\nRUN apt-get update\nRUN apt-get install --no-install-recommends -y wget unzip openssh-server openmpi-bin Ray && rm -rf /var/lib/apt/lists/*;\n\n#install prodigal\nRUN mkdir /opt/bin\nRUN wget -O /opt/bin/prodigal https://github.com/hyattpd/Prodigal/releases/download/v2.6.2/prodigal.linux\nRUN chmod a+x /opt/bin/prodigal\n\n#install bowtie\nRUN apt-get install --no-install-recommends -y bowtie2 && rm -rf /var/lib/apt/lists/*;\n\n#install samtools\nRUN apt-get install --no-install-recommends -y samtools && rm -rf /var/lib/apt/lists/*;\n\n#install java (needed for trimmomatic)\nRUN apt-get install --no-install-recommends -y openjdk-7-jre && rm -rf /var/lib/apt/lists/*;\n\n#install bedtools\nRUN apt-get install --no-install-recommends -y bedtools && rm -rf /var/lib/apt/lists/*;\n\n#install make\nRUN apt-get install -y --no-install-recommends make && rm -rf /var/lib/apt/lists/*;\n\n#set path\nENV PATH /opt/bin:$PATH\n\nWORKDIR /home/biogas\nENTRYPOINT [\"/bin/bash\",\"/home/biogas/run.sh\"]\n"
