  "repairedDockerfile": "# SOX audio tool \n#FROM ubuntu:14.04 \n#FROM meteorhacks/meteord:base \n#FROM kadirahq/meteord:base \nFROM abernix/meteord:base \nMAINTAINER Philipp Fauser <philipp@attic-studio.net> \nRUN apt-get update \n\n# install ffmpeg \nRUN apt-get install --no-install-recommends -y libav-tools && rm -rf /var/lib/apt/lists/*;\nRUN ln -s /usr/bin/avconv /usr/bin/ffmpeg \n\n# install sox and mp3 codec \nRUN apt-get install --no-install-recommends -y sox && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y libsox-fmt-mp3 && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y curl && rm -rf /var/lib/apt/lists/*;\n\n# create folders \nRUN mkdir /opt/files \n\n# get audio logo \nRUN curl -f https://files.noisefarm.net/files/logo.mp3 > /opt/files/logo.mp3\nRUN ls -la \n\n# change permissions \nRUN chmod a+wx /opt \nRUN chmod a+wx /opt/files \nRUN chmod a+wx /opt/files/logo.mp3 \n\n# install ID3 tag python lib \nRUN apt-get install --no-install-recommends -y python-pip && rm -rf /var/lib/apt/lists/*;\nRUN pip install --no-cache-dir mutagen\n\n"
