  "repairedDockerfile": "FROM ubuntu:16.04\n\nRUN sed -i \"s/http:\\/\\/archive.ubuntu.com/http:\\/\\/kr.archive.ubuntu.com/g\" /etc/apt/sources.list\nRUN apt-get update && apt-get -y dist-upgrade\n\nRUN apt-get install --no-install-recommends -yy tzdata && rm -rf /var/lib/apt/lists/*;\nENV TZ=Asia/Seoul\nRUN ln -snf /usr/share/zoneinfo/$TZ /etc/localtime && echo $TZ > /etc/timezone\n\nRUN apt-get install --no-install-recommends -yy socat && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -yy python && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -yy git && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -yy cython && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -yy wget && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -yy unzip && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -yy make && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -yy automake && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -yy libtool && rm -rf /var/lib/apt/lists/*;\n\nENV PROB readflag\nRUN useradd -m $PROB\n\nWORKDIR /tmp\nRUN wget https://github.com/seccomp/libseccomp/archive/master.zip\nRUN unzip master.zip\nWORKDIR /tmp/libseccomp-master/\nRUN ./autogen.sh\nRUN ./configure --build=\"$(dpkg-architecture --query DEB_BUILD_GNU_TYPE)\"\nRUN make\n\nWORKDIR /tmp/libseccomp-master/src/python\nRUN make build\nRUN make install\nRUN cp /usr/local/lib/python2.7/site-packages/seccomp.so /usr/local/lib/python2.7/dist-packages/\n\nCOPY ./start.sh /start.sh\nADD ./src/* /home/$PROB/\nRUN chmod +x /start.sh\n\nWORKDIR /home/$PROB\nCMD [\"/start.sh\"]\nEXPOSE 55402\n"
