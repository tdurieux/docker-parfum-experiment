  "repairedDockerfile": "# Get the Composer executable from the official Composer image.\nFROM composer:1.6 as composer\n\n# Run Baton in a small PHP container.\nFROM php:7.2\n\nRUN apt-get update\n\n# Install dockerize to be able to wait for the MySQL server.\nRUN apt-get install --no-install-recommends -y wget && rm -rf /var/lib/apt/lists/*;\nENV DOCKERIZE_VERSION v0.6.1\nRUN wget https://github.com/jwilder/dockerize/releases/download/$DOCKERIZE_VERSION/dockerize-linux-amd64-$DOCKERIZE_VERSION.tar.gz \\\n    && tar -C /usr/local/bin -xzvf dockerize-linux-amd64-$DOCKERIZE_VERSION.tar.gz \\\n    && rm dockerize-linux-amd64-$DOCKERIZE_VERSION.tar.gz\n\n# Install Composer and required extensions.\nCOPY --from=composer /usr/bin/composer /usr/bin/composer\nRUN apt-get install --no-install-recommends -y git zlib1g-dev \\\n    && docker-php-ext-install pdo pdo_mysql zip && rm -rf /var/lib/apt/lists/*;\n\n# install npm from nodesource using apt-get\nRUN apt-get install --no-install-recommends -my wget gnupg \\\n    && curl -f -sL https://deb.nodesource.com/setup_8.x | bash - \\\n    && apt-get install --no-install-recommends -yq nodejs build-essential && rm -rf /var/lib/apt/lists/*;\n\nRUN npm install -g npm && npm install -g gulp && npm cache clean --force;\n\n\nWORKDIR /usr/app/baton\n"
