  "repairedDockerfile": "FROM ubuntu:16.04\n\nARG USER_HOME_DIR=\"/root\"\n\nENV SCALA_VERSION 2.12.6\nENV SBT_VERSION 0.13.16\n\n# Update the repository sources list and install dependencies\nRUN apt-get update\n\nRUN apt-get install --no-install-recommends -y software-properties-common unzip htop rsync openssh-client jq && rm -rf /var/lib/apt/lists/*;\n\n# install Java\nUSER root\nRUN mkdir -p /usr/share/man/man1 && \\\n    apt-get update -y && \\\n    apt-get install --no-install-recommends -y openjdk-8-jdk && rm -rf /var/lib/apt/lists/*;\n\nRUN apt-get install --no-install-recommends unzip -y && \\\n    apt-get autoremove -y && rm -rf /var/lib/apt/lists/*;\n\n# Install Scala\n## Piping curl directly in tar\nRUN \\\n  apt-get install -y --no-install-recommends curl --assume-yes && \\\n  curl -fsL https://downloads.typesafe.com/scala/$SCALA_VERSION/scala-$SCALA_VERSION.tgz | tar xfz - -C /root/ && \\\n  echo >> /root/.bashrc && \\\n  echo 'export PATH=~/scala-$SCALA_VERSION/bin:$PATH' >> /root/.bashrc && rm -rf /var/lib/apt/lists/*;\n\n# Install sbt\nRUN \\\n  curl -f -L -o sbt-$SBT_VERSION.deb https://dl.bintray.com/sbt/debian/sbt-$SBT_VERSION.deb && \\\n  dpkg -i sbt-$SBT_VERSION.deb && \\\n  rm sbt-$SBT_VERSION.deb && \\\n  sbt sbtVersion\n\nVOLUME \"$USER_HOME_DIR/.sbt\"\n\n# Install nodejs\nRUN \\\n  curl -f -sL https://deb.nodesource.com/setup_8.x | bash -\n\nRUN \\\n  apt-get install -y --no-install-recommends nodejs --assume-yes && rm -rf /var/lib/apt/lists/*;\n\nENV LANG en_US.UTF-8\nENV LANGUAGE en_US:en\nENV LC_ALL en_US.UTF-8\nENV JAVA_TOOL_OPTIONS -Dfile.encoding=UTF8\n\n# Final user and home config\nRUN useradd --create-home --shell /bin/bash jenkins\nUSER jenkins\nWORKDIR /home/jenkins"
