  "repairedDockerfile": "FROM ubuntu:16.04\nRUN apt-get -y update\nRUN apt-get -y --no-install-recommends install apt-utils && rm -rf /var/lib/apt/lists/*;\nRUN apt-get -y --no-install-recommends install git wget sudo software-properties-common lsb-release && rm -rf /var/lib/apt/lists/*;\nRUN apt-get -y --no-install-recommends install mono-complete && rm -rf /var/lib/apt/lists/*;\n\n# build SMACK (with multi-language support PR)\nRUN cd /root && git clone https://github.com/smackers/smack.git -b develop\nRUN cd /root/smack/bin && ./build.sh\nENV BOOGIE=\"mono /root/boogie/Binaries/Boogie.exe\"\nENV CORRAL=\"mono /root/corral/bin/Release/corral.exe\"\n\n# install Bam\nRUN apt-get -y --no-install-recommends install ruby-dev && rm -rf /var/lib/apt/lists/*;\nRUN gem install bam-bam-boogieman bundler\n\n# install ctverif\nRUN apt-get -y --no-install-recommends install npm nodejs-legacy && rm -rf /var/lib/apt/lists/*;\nRUN npm install -g n && n lts && npm cache clean --force;\nRUN npm i -g ctverif && npm cache clean --force;\n\n# setup directory for running ctverif\nRUN mkdir /root/verifs\n"
