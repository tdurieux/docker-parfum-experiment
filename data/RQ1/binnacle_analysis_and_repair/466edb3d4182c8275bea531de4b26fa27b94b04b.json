  "repairedDockerfile": "FROM microsoft/dotnet:2.0-sdk \n\n#Install curl, supervisor, openssh-server, git \nRUN apt-get update && apt-get install --no-install-recommends -y curl openssh-server supervisor git && rm -rf /var/lib/apt/lists/*;\nRUN mkdir -p /var/run/sshd /var/log/supervisor \n\n#Install ncftp \nRUN apt-get install -y --no-install-recommends ncftp && rm -rf /var/lib/apt/lists/*;\n\n#Install node js \nRUN curl -f -sL https://deb.nodesource.com/setup_6.x | bash -\nRUN apt-get install --no-install-recommends -y nodejs && rm -rf /var/lib/apt/lists/*;\n\n#install ruby \nRUN apt-get install --no-install-recommends -y ruby-full && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y ruby-ffi && rm -rf /var/lib/apt/lists/*;\n\n#install sass \nRUN gem install sass \n\n#Install grunt \nRUN npm install -g grunt-cli && npm cache clean --force;\n\n#Install azure-cli \nRUN npm install -g azure-cli && npm cache clean --force;\n\n#Install vue-cli \nRUN npm install -g vue-cli && npm cache clean --force;\n\n#Install cross-env \nRUN npm install cross-env && npm cache clean --force;\n\n"
