  "repairedDockerfile": "# Docker file to build and test AIscm on different versions of Debian\n#\n# configure /etc/default/docker\n# DOCKER_OPTS=\"--dns 8.8.8.8 --dns 8.8.4.4 --ip-masq=true\"\nFROM debian:sid\nMAINTAINER Jan Wedekind <jan@wedesoft.de>\nRUN echo \"deb http://httpredir.debian.org/debian unstable main\" > /etc/apt/sources.list\nRUN apt-get update\nRUN apt-get -q -y dist-upgrade\nRUN apt-get install --no-install-recommends -q -y apt-utils && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -q -y build-essential && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -q -y autoconf automake libtool && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -q -y devscripts equivs && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -q -y linux-libc-dev && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -q -y guile-2.2-dev && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -q -y libx11-dev libxext-dev libxv-dev libxmu-dev libxi-dev libglu1-mesa-dev libgl1-mesa-dev && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -q -y gettext && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -q -y pandoc && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -q -y libjpeg-dev libmjpegtools-dev && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -q -y libpulse-dev && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -q -y imagemagick libmagickcore-dev && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -q -y libswresample-dev && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -q -y libswscale-dev && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -q -y libavutil-dev && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -q -y libavcodec-dev && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -q -y libavformat-dev && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -q -y libxpm-dev && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -q -y llvm-7 && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -q -y llvm-7-dev && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -q -y clang-7 && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -q -y libomp5 && rm -rf /var/lib/apt/lists/*;\nRUN mkdir -p /usr/src/aiscm && rm -rf /usr/src/aiscm\nWORKDIR /usr/src/aiscm\nADD debian/control debian/control\nRUN mk-build-deps --install --remove --tool 'apt-get -q --yes' debian/control\nCOPY aiscm.tar.xz .\nCOPY aiscm.tar.xz.asc .\nADD configure.ac .\nADD debian debian\nADD Makefile.package .\nRUN make -f Makefile.package\nRUN dpkg --install pkg/aiscm_*.deb\n"
