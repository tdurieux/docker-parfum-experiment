  "repairedDockerfile": "FROM ubuntu:16.04\n\n# Used to differentiate between continuous integration server and developer builds.\nARG CI\nENV CI ${CI:-false}\n\nRUN apt-get update\n\n# Installs compilers\n# - GCC\n# - Clang (set as default)\nRUN apt-get install --no-install-recommends -y g++-5 && \\\n    update-alternatives --remove g++ /usr/bin/g++ && \\\n    update-alternatives --install /usr/bin/g++ g++ /usr/bin/g++-5 100 && \\\n    update-alternatives --set g++ /usr/bin/g++-5 && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y clang-3.8 && \\\n    update-alternatives --remove clang++ /usr/bin/clang++ && \\\n    update-alternatives --install /usr/bin/clang++ clang++ /usr/bin/clang++-3.8 100 && \\\n    update-alternatives --set clang++ /usr/bin/clang++-3.8 && \\\n    update-alternatives --remove c++ /usr/bin/c++ && \\\n    update-alternatives --install /usr/bin/c++ c++ /usr/bin/clang++-3.8 100 && \\\n    update-alternatives --set c++ /usr/bin/clang++-3.8 && rm -rf /var/lib/apt/lists/*;\n\n# Installs test dependencies\n# - CMake\n# - Catch (unit tests)\nRUN apt-get install --no-install-recommends -y cmake && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y wget && \\\n    wget -O catch.tar.gz https://github.com/philsquared/Catch/archive/V1.5.0.tar.gz && \\\n    tar -xzf catch.tar.gz -C /usr/include --strip-components=2 Catch-1.5.0/single_include/catch.hpp && \\\n    rm -Rf catch.tar.gz catch/ && \\\n    apt-get remove -y --purge wget && \\\n    apt-get autoremove -y --purge && rm -rf /var/lib/apt/lists/*;\n\n# Installs documentation dependencies\n# - Python3 (documentation processing)\n# - Sphinx (documentation generation)\n# - Tinkerer (blog generation)\n# - Octave (image generation)\n# - ffmpeg (animation generation)\nRUN apt-get install --no-install-recommends -y python3 && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y python3-pip && \\\n    pip3 install --no-cache-dir --upgrade pip && \\\n    pip3 install --no-cache-dir Sphinx && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y liboctave-dev && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y ffmpeg && \\\n    apt-get install --no-install-recommends -y libavcodec-extra && rm -rf /var/lib/apt/lists/*;\n\n# Installs benchmark dependencies (excluded on CI servers)\n# - Docker\n# - Octave (already installed as documentation dependency)\nRUN if ! \"$CI\" -eq \"true\"; then \\\n      apt-get install --no-install-recommends -y apt-transport-https ca-certificates && \\\n      apt-key adv --keyserver hkp://p80.pool.sks-keyservers.net:80 --recv-keys 58118E89F3A912897C070ADBF76221572C52609D && \\\n      echo 'deb https://apt.dockerproject.org/repo ubuntu-xenial main' | tee /etc/apt/sources.list.d/docker.list && \\\n      apt-get update && \\\n      apt-get install --no-install-recommends -y docker-engine \\\n    ; rm -rf /var/lib/apt/lists/*; fi\n"
