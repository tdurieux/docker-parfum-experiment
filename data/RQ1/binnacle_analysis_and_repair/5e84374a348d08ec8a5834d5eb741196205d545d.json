  "repairedDockerfile": "FROM ubuntu\n\n# VERSION = 0.1\n\n# Set environment variables.\nENV HOME /root\n\nENV DEBIAN_FRONTEND noninteractive\n\n# Proxy setting\nENV http_proxy   $http_proxy\nENV https_proxy  $https_proxy\nENV all_proxy    $all_proxy\nENV no_proxy     $no_proxy\nENV HTTP_PROXY   $HTTP_PROXY\nENV HTTPS_PROXY  $HTTPS_PROXY\nENV ALL_PROXY    $ALL_PROXY\nENV NO_PROXY     $NO_PROXY\n\n# Define working directory.\nWORKDIR /root\n\nRUN apt update\n#RUN apt upgrade\n\nRUN apt install --no-install-recommends -y gcc && rm -rf /var/lib/apt/lists/*;\nRUN apt install --no-install-recommends -y build-essential && rm -rf /var/lib/apt/lists/*;\nRUN apt install --no-install-recommends -y libssl-dev && rm -rf /var/lib/apt/lists/*;\nRUN apt install --no-install-recommends -y libreadline6-dev && rm -rf /var/lib/apt/lists/*;\nRUN apt install --no-install-recommends -y libncurses5-dev && rm -rf /var/lib/apt/lists/*;\nRUN apt install --no-install-recommends -y libxml2-dev && rm -rf /var/lib/apt/lists/*;\nRUN apt install --no-install-recommends -y libxslt1-dev && rm -rf /var/lib/apt/lists/*;\nRUN apt install --no-install-recommends -y libpcre3 && rm -rf /var/lib/apt/lists/*;\nRUN apt install --no-install-recommends -y libpcre3-dev && rm -rf /var/lib/apt/lists/*;\nRUN apt install --no-install-recommends -y libev4 && rm -rf /var/lib/apt/lists/*;\nRUN apt install --no-install-recommends -y libev-dev && rm -rf /var/lib/apt/lists/*;\n\nRUN apt install --no-install-recommends -y vim-gnome && rm -rf /var/lib/apt/lists/*;\nRUN apt install --no-install-recommends -y git-core && rm -rf /var/lib/apt/lists/*;\nRUN apt install --no-install-recommends -y zsh && rm -rf /var/lib/apt/lists/*;\nRUN apt install --no-install-recommends -y jq && rm -rf /var/lib/apt/lists/*;\nRUN apt install --no-install-recommends -y wget && rm -rf /var/lib/apt/lists/*;\nRUN apt install --no-install-recommends -y curl && rm -rf /var/lib/apt/lists/*;\nRUN apt install --no-install-recommends -y tmux && rm -rf /var/lib/apt/lists/*;\nRUN apt install --no-install-recommends -y tree && rm -rf /var/lib/apt/lists/*;\n\n# Install Tools\nRUN apt install --no-install-recommends -y libgoogle-perftools-dev && rm -rf /var/lib/apt/lists/*;\nRUN apt install --no-install-recommends -y apache2-utils && rm -rf /var/lib/apt/lists/*;\n\n# Get dotfiles\nRUN git clone --depth 1 https://github.com/jxck/dotfiles  $DOTFILES\nRUN chsh -s /usr/bin/zsh\nRUN $DOTFILES/bin/slink.sh\n\nRUN $DOTFILES/install/install-weighttp.sh\n"
