  "repairedDockerfile": "# Install Ubuntu 18.04\n# Other supported cuda images: https://hub.docker.com/r/nvidia/cuda/\nFROM nvidia/cuda:10.0-runtime-ubuntu18.04\n\n# Install basic dependencies\nRUN apt-get update\nRUN apt-get install --no-install-recommends -y python3 python3-pip wget && rm -rf /var/lib/apt/lists/*;\n\n# OpenCV dependencies\nRUN apt-get install --no-install-recommends -y libglib2.0-0 && rm -rf /var/lib/apt/lists/*;\n\n# MPI dependencies\nRUN apt-get install --no-install-recommends -y ssh && rm -rf /var/lib/apt/lists/*;\nRUN wget https://download.open-mpi.org/release/open-mpi/v3.1/openmpi-3.1.2.tar.gz\nRUN tar -xvf openmpi-3.1.2.tar.gz && rm openmpi-3.1.2.tar.gz\nWORKDIR /openmpi-3.1.2\nRUN ./configure --build=\"$(dpkg-architecture --query DEB_BUILD_GNU_TYPE)\"\nRUN make -j 8 install\nWORKDIR /\n\n# Gym dependencies\nRUN apt-get install --no-install-recommends -y cmake zlib1g-dev libjpeg-dev && rm -rf /var/lib/apt/lists/*;\n\n# PyTorch 1.0\nRUN pip3 install --no-cache-dir https://download.pytorch.org/whl/cu100/torch-1.0.0-cp36-cp36m-linux_x86_64.whl\n\n# Set Python 3 as the default\nRUN ln -s /usr/bin/python3 /usr/bin/python\n\n# Install AdeptRL\nCOPY ./ /adeptRL\nWORKDIR /adeptRL\nRUN pip3 install --no-cache-dir .[all]\n"
