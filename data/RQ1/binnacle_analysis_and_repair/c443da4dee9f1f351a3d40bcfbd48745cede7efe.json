  "repairedDockerfile": "FROM ubuntu:wily\n\n# Install kerberos and unicode-data dependencies\nRUN apt-get install --no-install-recommends --yes libkrb5-dev && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends --yes unicode-data && rm -rf /var/lib/apt/lists/*;\n\n# Install git\nRUN apt-get install --no-install-recommends --yes git && rm -rf /var/lib/apt/lists/*;\n\n# Install node\nRUN apt-get install --no-install-recommends --yes curl && rm -rf /var/lib/apt/lists/*;\nRUN curl -f -sL https://deb.nodesource.com/setup_4.x | bash -\nRUN apt-get install --no-install-recommends --yes nodejs && rm -rf /var/lib/apt/lists/*;\nRUN node -v\nRUN npm -v\n\n# Create app directory\nRUN mkdir -p /opt/app\nWORKDIR /opt/app\n\n# Bundle app source\nCOPY . /opt/app\nRUN chmod 755 start.sh\n\n# Install app dependencies\nRUN npm install && npm cache clean --force;\n\nEXPOSE 3000\nENTRYPOINT [ \"/opt/app/start.sh\" ]\n"
