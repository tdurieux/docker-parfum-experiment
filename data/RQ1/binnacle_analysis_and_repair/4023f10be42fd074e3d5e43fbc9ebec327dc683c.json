  "repairedDockerfile": "FROM tleyden5iwx/caffe-gpu-master\n\nRUN apt-get update\nRUN apt-get -y --no-install-recommends install python-pip && rm -rf /var/lib/apt/lists/*;\nRUN apt-get -y --no-install-recommends install python-skimage && rm -rf /var/lib/apt/lists/*;\nRUN apt-get -y --no-install-recommends install yasm && rm -rf /var/lib/apt/lists/*;\nRUN pip install --no-cache-dir numpy matplotlib scipy\n\nWORKDIR /home\nRUN wget https://ffmpeg.org/releases/ffmpeg-3.0.1.tar.bz2\nRUN tar -xvf ffmpeg-3.0.1.tar.bz2 && rm ffmpeg-3.0.1.tar.bz2\nWORKDIR /home/ffmpeg-3.0.1\nRUN ./configure --build=\"$(dpkg-architecture --query DEB_BUILD_GNU_TYPE)\" && make && make install\n\nWORKDIR /home\nRUN git clone https://github.com/ackimball/autocolorization\nWORKDIR /home/autocolorization\n\nRUN wget https://eecs.berkeley.edu/~rich.zhang/projects/2016_colorization/files/demo_v0/colorization_release_v0.caffemodel\n\nRUN apt-get -y --no-install-recommends install libsnappy-dev && rm -rf /var/lib/apt/lists/*;\nRUN apt-get -y --no-install-recommends install libgraphicsmagick-dev && rm -rf /var/lib/apt/lists/*;\n\nRUN git clone https://github.com/torch/distro.git ~/torch --recursive\nWORKDIR /home/torch\nRUN bash install-deps\nRUN chmod 777 install.sh\nRUN ./install.sh\n\nWORKDIR /home\nRUN luarocks install graphicsmagick # upgrade\nRUN luarocks install lua-csnappy\nRUN luarocks install md5\nRUN luarocks install uuid\n\nRUN git clone --depth 1 https://github.com/nagadomi/waifu2x.git\nRUN mv waifu2x/* /home/autocolorization\n"
