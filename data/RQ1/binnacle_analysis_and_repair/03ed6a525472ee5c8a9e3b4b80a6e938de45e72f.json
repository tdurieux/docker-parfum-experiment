  "repairedDockerfile": "FROM ubuntu:16.04\n\nMAINTAINER Gildásio Júnior (gjuniioor@protonmail.com)\n\n# update package list\nRUN apt-get update\n\n# install curl and git\nRUN apt-get install --no-install-recommends -y curl git && rm -rf /var/lib/apt/lists/*;\n\n# install apache\nRUN apt-get install --no-install-recommends -y apache2 && rm -rf /var/lib/apt/lists/*;\n\n# install php\nRUN apt-get -y --no-install-recommends install php7.0 mcrypt php7.0-mcrypt php-mbstring php-pear php7.0-dev php7.0-xml && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y libapache2-mod-php7.0 && rm -rf /var/lib/apt/lists/*;\n\n# install pre requisites\nRUN apt-get update\nRUN apt-get install --no-install-recommends -y apt-transport-https && rm -rf /var/lib/apt/lists/*;\nRUN curl -f https://packages.microsoft.com/keys/microsoft.asc | apt-key add -\nRUN curl -f https://packages.microsoft.com/config/ubuntu/16.04/prod.list > /etc/apt/sources.list.d/mssql-release.list\nRUN apt-get update\nRUN ACCEPT_EULA=Y apt-get --no-install-recommends install -y msodbcsql mssql-tools && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y unixodbc-utf16 && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y unixodbc-dev-utf16 && rm -rf /var/lib/apt/lists/*;\n\n# install driver sqlsrv\nRUN pecl install sqlsrv\nRUN echo \"extension=sqlsrv.so\" >> /etc/php/7.0/apache2/php.ini\nRUN pecl install pdo_sqlsrv\nRUN echo \"extension=pdo_sqlsrv.so\" >> /etc/php/7.0/apache2/php.ini\n\n# load driver sqlsrv\nRUN echo \"extension=/usr/lib/php/20151012/sqlsrv.so\" >> /etc/php/7.0/apache2/php.ini\nRUN echo \"extension=/usr/lib/php/20151012/pdo_sqlsrv.so\" >> /etc/php/7.0/apache2/php.ini\nRUN echo \"extension=/usr/lib/php/20151012/sqlsrv.so\" >> /etc/php/7.0/cli/php.ini\nRUN echo \"extension=/usr/lib/php/20151012/pdo_sqlsrv.so\" >> /etc/php/7.0/cli/php.ini\n\n# install composer\nRUN php -r \"copy('https://getcomposer.org/installer', 'composer-setup.php');\"\nRUN php -r \"if (hash_file('SHA384', 'composer-setup.php') === '669656bab3166a7aff8a7506b8cb2d1c292f042046c5a994c43155c0be6190fa0355160742ab2e1c88d40d5be660b410') { echo 'Installer verified'; } else { echo 'Installer corrupt'; unlink('composer-setup.php'); } echo PHP_EOL;\"\nRUN php composer-setup.php\nRUN php -r \"unlink('composer-setup.php');\"\nRUN mv composer.phar /usr/local/bin/composer\n\n# install ODBC Driver\nRUN ACCEPT_EULA=Y apt-get --no-install-recommends install -y msodbcsql && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y mssql-tools && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y unixodbc-dev && rm -rf /var/lib/apt/lists/*;\nRUN echo 'export PATH=\"$PATH:/opt/mssql-tools/bin\"' >> ~/.bash_profile\nRUN echo 'export PATH=\"$PATH:/opt/mssql-tools/bin\"' >> ~/.bashrc\nRUN exec bash\n\n# install locales\nRUN apt-get install --no-install-recommends -y locales && echo \"en_US.UTF-8 UTF-8\" > /etc/locale.gen && locale-gen && rm -rf /var/lib/apt/lists/*;\n\nEXPOSE 80\n\nWORKDIR /var/www/html/\n\nCMD [\"/usr/sbin/apache2ctl\", \"-D\", \"FOREGROUND\"]\n"
