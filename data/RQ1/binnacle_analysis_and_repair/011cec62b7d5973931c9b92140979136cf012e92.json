  "repairedDockerfile": "FROM david6miji/gulp-tool:latest \n\nMAINTAINER David You <david6miji@gmail.com> \n\nENV DEBIAN_FRONTEND noninteractive \n\n# Install c build tool \nRUN apt-get install --no-install-recommends -y build-essential && rm -rf /var/lib/apt/lists/*;\n\n# Install avr build tool \nRUN apt-get install --no-install-recommends -y gcc-avr && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y avr-libc && rm -rf /var/lib/apt/lists/*;\n\n# Install elf tool \nRUN apt-get install --no-install-recommends -y libelf-dev && rm -rf /var/lib/apt/lists/*;\n\n# Install freeglut3-dev \nRUN apt-get install --no-install-recommends -y freeglut3-dev && rm -rf /var/lib/apt/lists/*;\n\n# Install gtkwave \nRUN apt-get install --no-install-recommends -y gtkwave && rm -rf /var/lib/apt/lists/*;\n\n# Install texinfo \nRUN apt-get install --no-install-recommends -y texinfo && rm -rf /var/lib/apt/lists/*;\n\n# Install ctags \nRUN apt-get install --no-install-recommends -y ctags && rm -rf /var/lib/apt/lists/*;\n\n# Install simavr \nWORKDIR /tmp/ \nRUN git clone git://github.com/buserror-uk/simavr.git \n\nWORKDIR /tmp/simavr \nRUN make all \nRUN make install \n\n# Define working directory. \nWORKDIR /work \n\n\n"
