  "repairedDockerfile": "FROM ubuntu:18.04\n\nWORKDIR /app\n\n# Add curl here\nRUN apt-get -y update && apt-get -y --no-install-recommends install openjdk-8-jdk hfst python3 python3-pip git curl wget && rm -rf /var/lib/apt/lists/*;\n\n# Install git-lfs repo\nRUN curl -f -s https://packagecloud.io/install/repositories/github/git-lfs/script.deb.sh | bash\n\n# Install git-lfs\nRUN apt-get -y --no-install-recommends install git-lfs && rm -rf /var/lib/apt/lists/*;\n\n# Use git lfs for clone to make the process foolproof\nRUN git lfs clone --recurse-submodules https://github.com/dlt-rilmta/e-magyar-tsv .\n# RUN git checkout 1c618d89493cda9510adecc9fcb6811636988e4c\n\nRUN pip3 install --no-cache-dir Cython\nRUN pip3 install --no-cache-dir -r emmorphpy/requirements.txt\nRUN pip3 install --no-cache-dir -r purepospy/requirements.txt\nRUN pip3 install --no-cache-dir -r emdeppy/requirements.txt\nRUN pip3 install --no-cache-dir -r HunTag3/requirements.txt\n\n# Install locales. Any UTF-8 locale will do.\nRUN apt-get -y --no-install-recommends install locales && rm -rf /var/lib/apt/lists/*;\nRUN sed -i -e 's/# en_US.UTF-8 UTF-8/en_US.UTF-8 UTF-8/' /etc/locale.gen && locale-gen\nENV LANG en_US.UTF-8\nENV LANGUAGE en_US:en\nENV LC_ALL en_US.UTF-8\n\nRUN make -C emtokenpy/ all\n\nCMD [\"python3\", \"./emtsv.py\", \"tok,morph,pos\"]\n"
