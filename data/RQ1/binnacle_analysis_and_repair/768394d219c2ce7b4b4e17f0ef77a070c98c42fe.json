  "repairedDockerfile": "FROM ubuntu:trusty \n\nRUN apt-get update \n\nRUN apt-get install --no-install-recommends -y build-essential nodejs npm && rm -rf /var/lib/apt/lists/*;\nRUN ln -s $(which nodejs) /usr/bin/node \n\nRUN apt-get install --no-install-recommends -y spamassassin spamc && rm -rf /var/lib/apt/lists/*;\n\nRUn apt-get install --no-install-recommends -y rsyslog && rm -rf /var/lib/apt/lists/*;\n\n# Install git, curl, python, and phantomjs \nRUN apt-get install --no-install-recommends -y git curl python phantomjs && rm -rf /var/lib/apt/lists/*;\n\n# Make sure we have a directory for the application \nRUN mkdir -p /var/www \nRUN chown -R www-data:www-data /var/www \n\n# Install fibers -- this doesn't seem to do any good, for some reason \nRUN npm install -g fibers && npm cache clean --force;\n\n# Install Mailin \nRUN npm install -g mailin && npm cache clean --force;\n\n# Install Meteor \nRUN curl -f https://install.meteor.com/ | sh\n\n# Install entrypoint \nADD entrypoint.sh /usr/bin/entrypoint.sh \nRUN chmod +x /usr/bin/entrypoint.sh \n\nEXPOSE 80 \nEXPOSE 25 \nENTRYPOINT [\"/usr/bin/entrypoint.sh\"] \nCMD []\n\n"
