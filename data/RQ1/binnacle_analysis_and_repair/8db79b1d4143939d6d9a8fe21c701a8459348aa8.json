  "repairedDockerfile": "FROM ubuntu:14.04\nMAINTAINER Li XiaoDong\n\nRUN apt-get update\n\n# To compile and install native addons from npm\nRUN sudo apt-get install --no-install-recommends -y build-essential && rm -rf /var/lib/apt/lists/*;\n\n# install nodejs\n# nodejs 6.X\nRUN curl -f -sL https://deb.nodesource.com/setup_6.x | sudo -E bash -\n# nodejs 7.X\n# RUN curl -sL https://deb.nodesource.com/setup_7.x | sudo -E bash -\n\nRUN sudo apt-get install --no-install-recommends -y nodejs && rm -rf /var/lib/apt/lists/*;\n\n# install git\nRUN sudo apt-get install --no-install-recommends -y git && rm -rf /var/lib/apt/lists/*;\n\n# install zeromq\nRUN sudo apt-get install --no-install-recommends wget -yq && rm -rf /var/lib/apt/lists/*;\nRUN sudo apt-get install --no-install-recommends libtool pkg-config build-essential autoconf automake -yq && rm -rf /var/lib/apt/lists/*;\nRUN sudo apt-get install --no-install-recommends python -yq && rm -rf /var/lib/apt/lists/*;\nRUN sudo apt-get install -y --no-install-recommends libzmq3-dev && rm -rf /var/lib/apt/lists/*;\nRUN sudo npm i zmq -g && npm cache clean --force;\n\n# install redis and start\nRUN sudo apt-get install --no-install-recommends -y redis-server && rm -rf /var/lib/apt/lists/*;\nRUN sudo /etc/init.d/redis-server start\n\n# install mongodb and start\nRUN sudo apt-get install --no-install-recommends -y mongodb && rm -rf /var/lib/apt/lists/*;\nRUN sudo /etc/init.d/mongod start\n\n# set cnpm\nRUN npm install -g cnpm --registry=https://r.cnpmjs.org && npm cache clean --force;\n\n# 修改时区\nRUN cp /usr/share/zoneinfo/Asia/Shanghai /etc/localtime\n\n# install pomelo\nRUN sudo npm i pomelo -g && npm cache clean --force;\n\n\n\n\n\n\n"
