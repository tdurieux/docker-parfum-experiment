  "repairedDockerfile": "# We use debian as our base distro for this container.\nFROM debian:latest\n\n# Refresh apt-get.\nRUN apt-get update\n\n# Install some utilities needed by node, npm, and ZeroMQ.\nRUN apt-get install --no-install-recommends -y curl make g++ && rm -rf /var/lib/apt/lists/*;\n\n# Install Node.js and npm.\nRUN curl -f -sL https://deb.nodesource.com/setup | bash -\nRUN apt-get install --no-install-recommends -y nodejs && rm -rf /var/lib/apt/lists/*;\n\n# Install ZeroMQ libs.\nRUN apt-get install --no-install-recommends -y libzmq-dev && rm -rf /var/lib/apt/lists/*;\n\n# Install some perf test tools.\nRUN apt-get install --no-install-recommends -y fio iperf && rm -rf /var/lib/apt/lists/*;\n\n# Install required npm packages.\nADD package.json /package.json\nRUN npm install && npm cache clean --force;\n\n# Set /src as the working directory for this container.\nWORKDIR /src\n\n# Open up external access to port 80.\nEXPOSE  80\n\n# Run startup command.\nCMD [\"node\", \"/src/scripts/server.js\"]\n"
