  "repairedDockerfile": "FROM tleyden5iwx/stroke-width-transform\n\nRUN apt-get -q -y update\n\nENV GOPATH /opt/go\nRUN mkdir -p $GOPATH\n\nRUN apt-get -q --no-install-recommends -y install git && rm -rf /var/lib/apt/lists/*;\nRUN apt-get -q --no-install-recommends -y install golang && rm -rf /var/lib/apt/lists/*;\nRUN apt-get -q --no-install-recommends -y install gcc && rm -rf /var/lib/apt/lists/*;\n\n# since we are gonna install open-ocr, we need all the tesseract / leptonica deps\nRUN apt-get -q --no-install-recommends -y install libleptonica-dev && rm -rf /var/lib/apt/lists/*;\nRUN apt-get -q --no-install-recommends -y install libtesseract3 libtesseract-dev && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -q -y tesseract-ocr-eng && rm -rf /var/lib/apt/lists/*;\n\nRUN go get -u -v github.com/tleyden/open-ocr\n\n# build open-ocr-preprocessor binary and copy it to /usr/bin\nRUN cd $GOPATH/src/github.com/tleyden/open-ocr/cli-preprocessor && go build -v -o open-ocr-preprocessor && cp open-ocr-preprocessor /usr/bin\n\n"
