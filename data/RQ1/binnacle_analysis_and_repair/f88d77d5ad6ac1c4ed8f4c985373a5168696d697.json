  "repairedDockerfile": "# It Builds an image to deploy a Rserve\n\n# Dotcloud ubuntu image\nFROM ubuntu:13.10\nMAINTAINER Felipe Triana, ftrianakast@gmail.com\n\n# Update\nRUN apt-get update\n\n# Install wget\nRUN apt-get install --no-install-recommends -y wget && rm -rf /var/lib/apt/lists/*;\n\n# Install latest R\nRUN sh -c 'echo \"deb http://cran.rstudio.com/bin/linux/ubuntu trusty/\" >> /etc/apt/sources.list'\nRUN gpg --batch --keyserver keyserver.ubuntu.com --recv-key E084DAB9\nRUN gpg --batch -a --export E084DAB9 | sudo apt-key add -\nRUN apt-get update\nRUN sudo apt-get -y --no-install-recommends install r-base && rm -rf /var/lib/apt/lists/*;\n\n# Important additional libraries\nRUN apt-get install --no-install-recommends -y libcurl4-gnutls-dev && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install -y --no-install-recommends libxml2-dev && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y libmime-base64-urlsafe-perl libdigest-hmac-perl libdigest-sha-perl && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install -y --no-install-recommends libssl-dev && rm -rf /var/lib/apt/lists/*;\n\n# adding start R script: you can find the RScript on the docker github\nADD start.R start.R\nADD Rserv.conf /Rserv.conf\nEXPOSE 6311\nCMD Rscript start.R\n"
