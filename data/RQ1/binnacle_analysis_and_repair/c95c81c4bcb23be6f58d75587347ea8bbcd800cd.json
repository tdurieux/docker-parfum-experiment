  "repairedDockerfile": "FROM ubuntu:16.04 \nMAINTAINER Abdul Somat Budiaji \"abdulbudiaji@gmail.com\" \nRUN apt-get update && apt-get install --no-install-recommends -y \\\ngcc g++ \\\npython python-dev python-pip \\\ngettext \\\nmysql-client libmysqlclient-dev \\\nsqlite3 \\\n\\--no-install-recommends && rm -rf /var/lib/apt/lists/*;\n\nRUN pip install --no-cache-dir --upgrade pip\n\n# setuptools required to install matplotlib \nRUN pip install --no-cache-dir setuptools==25.1.1\n\n# freetype and png is required by matplotlib \nRUN apt-get install --no-install-recommends -y \\\nlibpng12-dev libfreetype6-dev pkg-config \\\n\\--no-install-recommends && rm -rf /var/lib/apt/lists/*;\n\n# libxml and libxslt is required by newspaper \nRUN apt-get install --no-install-recommends -y \\\nlibxml2-dev libxslt1-dev \\\n\\--no-install-recommends && rm -rf /var/lib/apt/lists/*;\n\n# required by gdal \nRUN apt-get install --no-install-recommends -y \\\nlibgdal-dev && rm -rf /var/lib/apt/lists/*;\n\nRUN pip install --no-cache-dir numpy==1.11.0#whl\nRUN pip install --no-cache-dir scipy==0.17.1#whl\nRUN pip install --no-cache-dir matplotlib==1.5.1#source\nRUN pip install --no-cache-dir nltk==2.0.5#source\nRUN pip install --no-cache-dir newspaper==0.0.9.8#source\n\n# install gdal python binding \nENV CPLUS_INCLUDE_PATH /usr/include/gdal \nENV C_INCLUDE_PATH /usr/include/gdal \nRUN pip install --no-cache-dir GDAL==1.11.2#source\n\n# install some tools \nRUN pip install --no-cache-dir beanstalkc==0.4.0#whl\nRUN pip install --no-cache-dir googlemaps==2.4.3#whl\nRUN pip install --no-cache-dir sendgrid==3.0.0#whl\nRUN pip install --no-cache-dir nose==1.3.7#whl\nRUN pip install --no-cache-dir path.py==8.2.1#whl\nRUN pip install --no-cache-dir xlrd==1.0.0#source\nRUN pip install --no-cache-dir xlwt==1.1.2#whl\nRUN pip install --no-cache-dir schedule==0.3.2#source\nRUN pip install --no-cache-dir itsdangerous==0.24#source\nRUN pip install --no-cache-dir psutil==4.3.0#source\nRUN pip install --no-cache-dir pika==0.10.0#whl\nRUN pip install --no-cache-dir minio==2.0.0#whl\n\n# install cython \nRUN pip install --no-cache-dir Cython==0.24.1#whl\n\n# related to django web development \nRUN pip install --no-cache-dir mysqlclient==1.3.7#source\nRUN pip install --no-cache-dir Django==1.9.7#whl\nRUN pip install --no-cache-dir djangorestframework==3.4.0# whl\nRUN pip install --no-cache-dir drfdocs==0.0.11#source\nRUN pip install --no-cache-dir whitenoise==3.2#whl\nRUN pip install --no-cache-dir Markdown==2.6.6#source\nRUN pip install --no-cache-dir gunicorn==19.6.0#whl\nRUN pip install --no-cache-dir luigi==2.2.0#source\nRUN pip install --no-cache-dir cassandra-driver==3.5.0#source\nRUN pip install --no-cache-dir reportlab==3.3.0#source\n\n# install supervisor \nRUN pip install --no-cache-dir supervisor==3.3.0#source\n\n# install frequently used locale \nRUN locale-gen id_ID.utf8 \nRUN locale-gen en_US.utf8 \nRUN update-locale \n\n# clean up \nRUN rm -rf /root/.cache/pip \nRUN rm -rf /var/lib/apt/lists/* \n\n"
