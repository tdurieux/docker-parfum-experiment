  "repairedDockerfile": "FROM ubuntu:16.04\n\nRUN apt-get update && \\\n    apt-get install --no-install-recommends git -y && \\\n    apt-get install --no-install-recommends python -y && \\\n    apt-get install --no-install-recommends python3 -y && \\\n    apt-get install --no-install-recommends python-pip -y && \\\n    apt-get install --no-install-recommends python3-pip -y && \\\n    apt-get install --no-install-recommends python-dev -y && \\\n    apt-get install --no-install-recommends python3-dev -y && \\\n    apt-get install --no-install-recommends build-essential -y && \\\n    apt-get install --no-install-recommends libssl-dev -y && \\\n    apt-get install --no-install-recommends libffi-dev -y && \\\n    apt-get install --no-install-recommends libxml2-dev -y && \\\n    apt-get install --no-install-recommends libxslt1-dev -y && \\\n    apt-get install --no-install-recommends zlib1g-dev -y && rm -rf /var/lib/apt/lists/*;\n\nRUN git clone https://github.com/mvelazc0/PurpleSpray.git /tmp/purplespray\n\nWORKDIR /tmp/purplespray\n\nRUN pip3 install --no-cache-dir -r requirements.txt\nRUN pip install --no-cache-dir -r requirements.txt\nRUN pip install --no-cache-dir requests\nRUN pip install --no-cache-dir impacket\nRUN rm -r /root/.cache\n\nENTRYPOINT [ \"python\", \"PurpleSpray.py\" ]"
