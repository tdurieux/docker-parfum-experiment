  "repairedDockerfile": "FROM ubuntu:16.04\nMAINTAINER Khanh Ngo \"k@ndk.name\"\nARG ENVIRONMENT=development\nENV ENVIRONMENT=${ENVIRONMENT}\nWORKDIR /powerdns-admin\n\nRUN apt-get update -y\nRUN apt-get install --no-install-recommends -y apt-transport-https && rm -rf /var/lib/apt/lists/*;\n\nRUN apt-get install --no-install-recommends -y locales locales-all && rm -rf /var/lib/apt/lists/*;\nENV LC_ALL en_US.UTF-8\nENV LANG en_US.UTF-8\nENV LANGUAGE en_US.UTF-8\n\nRUN apt-get install --no-install-recommends -y python3-pip python3-dev supervisor curl mysql-client && rm -rf /var/lib/apt/lists/*;\n\n# Install node 10.x\nRUN curl -f -sL https://deb.nodesource.com/setup_10.x | bash -\nRUN apt-get install --no-install-recommends -y nodejs && rm -rf /var/lib/apt/lists/*;\n\nRUN curl -f -sS https://dl.yarnpkg.com/debian/pubkey.gpg | apt-key add -\nRUN echo \"deb https://dl.yarnpkg.com/debian/ stable main\" > /etc/apt/sources.list.d/yarn.list\n\n# Install yarn\nRUN apt-get update -y\nRUN apt-get install --no-install-recommends -y yarn && rm -rf /var/lib/apt/lists/*;\n\n# Install Netcat for DB healthcheck\nRUN apt-get install --no-install-recommends -y netcat && rm -rf /var/lib/apt/lists/*;\n\n# lib for building mysql db driver\nRUN apt-get install --no-install-recommends -y libmysqlclient-dev && rm -rf /var/lib/apt/lists/*;\n\n# lib for building ldap and ssl-based application\nRUN apt-get install --no-install-recommends -y libsasl2-dev libldap2-dev libssl-dev && rm -rf /var/lib/apt/lists/*;\n\n# lib for building python3-saml\nRUN apt-get install --no-install-recommends -y libxml2-dev libxslt1-dev libxmlsec1-dev libffi-dev pkg-config && rm -rf /var/lib/apt/lists/*;\n\nCOPY ./requirements.txt /powerdns-admin/requirements.txt\nRUN pip3 install --no-cache-dir -r requirements.txt\n\nADD ./supervisord.conf /etc/supervisord.conf\nADD . /powerdns-admin/\nCOPY ./configs/${ENVIRONMENT}.py /powerdns-admin/config.py\nCOPY ./docker/PowerDNS-Admin/entrypoint.sh /entrypoint.sh\n\nENTRYPOINT [\"/entrypoint.sh\"]\n"
