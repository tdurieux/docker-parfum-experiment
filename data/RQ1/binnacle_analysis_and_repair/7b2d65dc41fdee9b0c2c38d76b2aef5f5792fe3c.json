  "repairedDockerfile": "FROM alanz/haskell-platform-2013.2-deb64\n\nMAINTAINER alan.zimm@gmail.com\n\nENV DEBIAN_FRONTEND noninteractive\n\n#-----------------------------------------------------------------------\n# Install emacs and ssh server\nRUN echo \"deb http://cdn.debian.net/debian/ testing main non-free contrib\" >> /etc/apt/sources.list\nRUN apt-get update\nRUN apt-get -y dist-upgrade\nRUN apt-get -y --no-install-recommends install ssh openssh-server && rm -rf /var/lib/apt/lists/*;\nRUN apt-get -y --no-install-recommends install sudo && rm -rf /var/lib/apt/lists/*;\nRUN apt-get -y --no-install-recommends install git && rm -rf /var/lib/apt/lists/*;\nRUN apt-get -y --no-install-recommends install emacs24 && rm -rf /var/lib/apt/lists/*;\n\n# Copy the files into the container\nADD . /src\n\nRUN /src/setup.sh\n\n#RUN /src/startssh.sh\n\n# Expose the ssh port\nEXPOSE 22\n# Start shell and ssh services.\nCMD [\"/bin/bash\",\"/src/startssh.sh\"]\n\n# Note: to debug the partial build do the following\n# docker run -i -t alanz/HaRe-7.6.3-64 /bin/bash\n"
