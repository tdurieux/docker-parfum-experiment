  "repairedDockerfile": "FROM ubuntu:14.04 \nMAINTAINER Alejandro Casado Quijada <acasadoquijada@gmail.com> \n\n#Instalamos git \nRUN sudo apt-get -y update \nRUN sudo apt-get install --no-install-recommends -y git && rm -rf /var/lib/apt/lists/*;\n\n#Clonamos el repositorio \nRUN sudo git clone https://github.com/acasadoquijada/IV.git \n\n#Instalamos las herramientas de python3 necesarias \nRUN sudo apt-get install --no-install-recommends -y python3-setuptools && rm -rf /var/lib/apt/lists/*;\nRUN sudo apt-get -y --no-install-recommends install python3-dev && rm -rf /var/lib/apt/lists/*;\nRUN sudo apt-get -y --no-install-recommends install build-essential && rm -rf /var/lib/apt/lists/*;\nRUN sudo apt-get -y --no-install-recommends install python-psycopg2 && rm -rf /var/lib/apt/lists/*;\nRUN sudo apt-get -y --no-install-recommends install libpq-dev && rm -rf /var/lib/apt/lists/*;\nRUN sudo easy_install3 pip \nRUN sudo pip install --upgrade pip \n\n#Instalamos los requerimientos necesarios \nRUN cd IV/ && make install \n\n#Realizamos la sincronizacion entre las BD \nRUN cd IV/ && python3 manage.py syncdb --noinput \n\n"
