  "repairedDockerfile": "from ubuntu:14.04.5\n\nmaintainer E-ARK project, http://www.eark-project.com\n\nRUN echo \"deb http://archive.ubuntu.com/ubuntu/ trusty multiverse\" >> /etc/apt/sources.list\nRUN apt-get update\n\n# install python\nRUN apt-get install --no-install-recommends python --force-yes -y && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends python-setuptools --force-yes -y && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends build-essential --force-yes -y && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends python-virtualenv --force-yes -y && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends python-dev --force-yes -y && rm -rf /var/lib/apt/lists/*;\n\nRUN easy_install pip\n\nRUN apt-get install --no-install-recommends -y summain jhove pdftohtml && rm -rf /var/lib/apt/lists/*;\n\nRUN apt-get install --no-install-recommends -y graphicsmagick imagemagick && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y libgraphicsmagick++1-dev libboost-python-dev && rm -rf /var/lib/apt/lists/*;\n\nRUN apt-get install --no-install-recommends -y tar git curl vim wget dialog net-tools build-essential && rm -rf /var/lib/apt/lists/*;\n\nRUN wget https://github.com/openpreserve/fido/archive/1.3.2-81.tar.gz\nRUN tar -xzvf 1.3.2-81.tar.gz && rm 1.3.2-81.tar.gz\nRUN cd fido-1.3.2-81 && python setup.py install\n\n#RUN cd fido-1.3.2-81/fido && echo 'yes' | python update_signatures.py\n\nRUN wget https://downloads.ghostscript.com/public/old-gs-releases/ghostscript-9.18.tar.gz\nRUN tar -xzf ghostscript-9.18.tar.gz && rm ghostscript-9.18.tar.gz\nRUN cd ghostscript-9.18 && ./configure --build=\"$(dpkg-architecture --query DEB_BUILD_GNU_TYPE)\" && make && make install\n\nRUN apt-get install --no-install-recommends -y libgeos-dev libmysqlclient-dev libxml2-dev libxslt1-dev && rm -rf /var/lib/apt/lists/*;\n\nADD docker/wait-for-it/wait-for-it.sh /wait-for-it.sh\n\nADD requirements.txt /requirements.txt\nRUN pip install --no-cache-dir -r /requirements.txt\n\nRUN pip install --no-cache-dir redis\n\nRUN mkdir -p /var/data/earkweb\n\nRUN mkdir -p /var/log/earkweb\n\n"
