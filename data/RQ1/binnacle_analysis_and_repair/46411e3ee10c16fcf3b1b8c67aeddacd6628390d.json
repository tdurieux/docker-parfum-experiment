  "repairedDockerfile": "FROM       ubuntu:latest\nMAINTAINER Ivo Camargo <ivo.camargo@neoway.com.br>\n\n# Update apt-get sources AND install Python-dve\nRUN apt-get update && apt-get install --no-install-recommends -y build-essential python-dev && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y python-setuptools && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y python-pip && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y nano && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y telnet && rm -rf /var/lib/apt/lists/*;\n\n## Flask Extensions\nRUN pip install --no-cache-dir flask-wtf\nRUN pip install --no-cache-dir flask-bootstrap\nRUN pip install --no-cache-dir flask-pymongo\nRUN pip install --no-cache-dir flask-mail\n\n\n\n## Install a few other tools, nano, telnet and so on...\nRUN apt-get install --no-install-recommends -y nano && apt-get install --no-install-recommends -y telnet && rm -rf /var/lib/apt/lists/*;\n\nEXPOSE 5000\n\nENTRYPOINT [\"/usr/bin/python\" , \"/app/app.py\"]\n"
