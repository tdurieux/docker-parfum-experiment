  "repairedDockerfile": "FROM ubuntu:17.10\n\nMAINTAINER Denis Torre <denis.torre@mssm.com>\n\nRUN apt-get update && apt-get install --no-install-recommends -y python3 && rm -rf /var/lib/apt/lists/*;\nRUN apt-get update && apt-get install --no-install-recommends -y python3-pip && rm -rf /var/lib/apt/lists/*;\nRUN apt-get update && apt-get install --no-install-recommends -y python3-dev && rm -rf /var/lib/apt/lists/*;\nRUN apt-get update && apt-get install --no-install-recommends -y libmysqlclient-dev && rm -rf /var/lib/apt/lists/*;\n\nRUN pip3 install --no-cache-dir numpy\nRUN pip3 install --no-cache-dir pandas\nRUN pip3 install --no-cache-dir Flask\nRUN pip3 install --no-cache-dir sqlalchemy\nRUN pip3 install --no-cache-dir flask-sqlalchemy\nRUN pip3 install --no-cache-dir pymysql\nRUN pip3 install --no-cache-dir google-cloud\nRUN pip3 install --no-cache-dir h5py\n\nRUN apt-get update && apt-get install --no-install-recommends -y python3-setuptools && rm -rf /var/lib/apt/lists/*;\nRUN apt-get update && apt-get install --no-install-recommends -y nginx uwsgi-core && rm -rf /var/lib/apt/lists/*;\nRUN apt-get update && apt-get install --no-install-recommends -y libpcre3 libpcre3-dev && rm -rf /var/lib/apt/lists/*;\nRUN pip3 install --no-cache-dir uwsgi\nRUN pip3 install --no-cache-dir xlrd\n\nRUN mkdir biojupies-gtex\nCOPY . /biojupies-gtex\nWORKDIR /biojupies-gtex\nRUN chmod +x boot.sh; chmod -R 777 /biojupies-gtex/app/static;\n\nENTRYPOINT ./boot.sh"
