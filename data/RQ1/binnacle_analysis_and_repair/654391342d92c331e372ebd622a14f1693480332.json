  "repairedDockerfile": "FROM stackbrew/ubuntu:13.10\n\nMAINTAINER ahunnargikar@ebay.com\n\nRUN apt-get update\n\n#Install Java & Maven\nRUN apt-get -y --no-install-recommends install default-jdk maven && rm -rf /var/lib/apt/lists/*;\n\n#Install packages\nRUN apt-get -y --no-install-recommends install wget curl git-core nano && rm -rf /var/lib/apt/lists/*;\n\n# This will use the latest public release. To use your own, comment it out...\nADD https://get.docker.io/builds/Linux/x86_64/docker-latest /usr/local/bin/docker\n\n#Install & configure Supervisor\nRUN apt-get -y --no-install-recommends install supervisor && rm -rf /var/lib/apt/lists/*;\nRUN mkdir -p /var/log/supervisor\nADD supervisord.conf /etc/supervisor/conf.d/supervisord.conf\n\n#Make the Docker binary executable\nRUN chmod +x /usr/local/bin/docker\n\n#Run Supervisor\nCMD [\"/usr/bin/supervisord\"]"
