  "repairedDockerfile": "FROM ruby:2.3-slim \n\nMAINTAINER Dru Jensen <drujensen@gmail.com> \n\n# update package manager \nRUN apt-get update -qq \n\n# c libraries needed for most dev libraries \nRUN apt-get install --no-install-recommends -y build-essential git tmux && rm -rf /var/lib/apt/lists/*;\n\n# install latest nodejs packages \nRUN curl -f -sL https://deb.nodesource.com/setup_4.x | bash -\n\n# install nodejs \nRUN apt-get install --no-install-recommends -y nodejs && rm -rf /var/lib/apt/lists/*;\n\n# for ssl \nRUN apt-get install --no-install-recommends -y libssl-dev && rm -rf /var/lib/apt/lists/*;\n\n# for postgres \nRUN apt-get install --no-install-recommends -y libpq-dev && rm -rf /var/lib/apt/lists/*;\n\n# for mysql \nRUN apt-get install --no-install-recommends -y libmysqlclient-dev && rm -rf /var/lib/apt/lists/*;\n\n# for nokogiri \nRUN apt-get install --no-install-recommends -y libxml2-dev libxslt1-dev && rm -rf /var/lib/apt/lists/*;\n\n# for capybara-webkit \nRUN apt-get install --no-install-recommends -y libqt4-webkit libqt4-dev xvfb && rm -rf /var/lib/apt/lists/*;\n\n# for paperclip \nRUN apt-get install --no-install-recommends -y imagemagick && rm -rf /var/lib/apt/lists/*;\n\n\n"
