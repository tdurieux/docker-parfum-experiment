  "repairedDockerfile": "FROM ubuntu:12.04\nMAINTAINER Daekwon Kim <propellerheaven@gmail.com>\n\n# Run upgrades\nRUN echo \"deb http://archive.ubuntu.com/ubuntu precise main universe\" > /etc/apt/sources.list\nRUN apt-get update\n\n# Install basic packages\nRUN apt-get -qq --no-install-recommends -y install git curl build-essential && rm -rf /var/lib/apt/lists/*;\n\n# Install Ruby 2.0\nRUN apt-get -qq --no-install-recommends -y install python-software-properties && rm -rf /var/lib/apt/lists/*;\nRUN apt-add-repository ppa:brightbox/ruby-ng\nRUN apt-get update\nRUN apt-get -qq --no-install-recommends -y install ruby2.0 ruby2.0-dev && rm -rf /var/lib/apt/lists/*;\nRUN gem install bundler --no-ri --no-rdoc\n\n# Install packages for installing Huboard\nRUN apt-get -qq --no-install-recommends -y install couchdb memcached nodejs && rm -rf /var/lib/apt/lists/*;\nRUN apt-get clean\nRUN gem install foreman\n\n# Install Huboard\nRUN git clone -b master https://github.com/rauhryan/huboard.git /app\nRUN cd /app; bundle install;\nADD .env /app/.env\nADD Procfile /app/Procfile\n\n# Run Huboard instance\nEXPOSE 5000\nCMD foreman start -f /app/Procfile\n"
