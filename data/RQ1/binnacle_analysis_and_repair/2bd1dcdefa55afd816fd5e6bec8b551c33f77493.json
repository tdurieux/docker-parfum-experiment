  "repairedDockerfile": "FROM ubuntu:latest\nMAINTAINER Sreeja Kamishetty <sreeja25kamishetty@gmail.com>\n\nENV DEBIAN_FRONTEND noninteractive\n\n#Update\nRUN apt-get update\nRUN apt-get upgrade -y\n\n#Install proper dependencies\nRUN apt-get install --no-install-recommends -y git wget && rm -rf /var/lib/apt/lists/*;\n\n#run the script\nRUN apt-get install --no-install-recommends -y apache2 && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install -y --no-install-recommends mysql-server php5-mysql && rm -rf /var/lib/apt/lists/*;\nRUN  mysql_install_db\nRUN apt-get install -y --no-install-recommends php5 libapache2-mod-php5 php5-mcrypt && rm -rf /var/lib/apt/lists/*;\nRUN service apache2 restart\nRUN apt-cache search php5-cgi\n\n#Clone the source\nRUN git clone https://github.com/fossasia/engelsystem.git\n\n#cd into the dir\nWORKDIR engelsystem\n#add the mysql root password\nCOPY config/config.default.php config/config.php\nRUN mysql -u root -p engelsystem < db/install.sql\nRUN mysql -u root -p engelsystem < db/update.sql\nEXPOSE 80\n"
