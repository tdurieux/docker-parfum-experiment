  "repairedDockerfile": "# FE apache php base container \n# base off of ubuntu for now \nFROM ubuntu \n\nMAINTAINER Hinling Yeung \n\n# Make DEBIAN_FRONTEND less chatty \nENV DEBIAN_FRONTEND noninteractive \nENV DEBIAN_FRONTEND newt \n\n# Update the default application repository sources list \nRUN apt-get update \n\n# Install apache 2 \nRUN apt-get -y --no-install-recommends install apache2 && rm -rf /var/lib/apt/lists/*;\n\n# Install php5 \nRUN apt-get -y --no-install-recommends install php5 libapache2-mod-php5 php5-dev && rm -rf /var/lib/apt/lists/*;\n\n# Install mysql client \nRUN apt-get -y --no-install-recommends install libapache2-mod-auth-mysql php5-mysql mysql-client && rm -rf /var/lib/apt/lists/*;\n\n# Install PEAR \nRUN apt-get -y --no-install-recommends install php-pear && rm -rf /var/lib/apt/lists/*;\n\n# Install phpmods \nRUN pear upgrade \\--force pear \nRUN printf \"no\\n\" | pecl install stomp-1.0.5 \nRUN apt-get -y --no-install-recommends install libpcre3-dev && rm -rf /var/lib/apt/lists/*;\nRUN apt-get -y --no-install-recommends install pkg-config && rm -rf /var/lib/apt/lists/*;\n# RUN pecl install APC -- no APC for php5.5 \nRUN pecl install xdebug \n# RUN pecl install memcached \n# Install apache libs \nRUN apt-get -y --no-install-recommends install apache2-dev apache2-doc && rm -rf /var/lib/apt/lists/*;\n\n# Install all the php libraries \nRUN apt-get -y --no-install-recommends install php5-common libapache2-mod-php5 php5-cli && rm -rf /var/lib/apt/lists/*;\n#RUN apt-get -y install php-gd \nRUN apt-get -y --no-install-recommends install php5-mcrypt && rm -rf /var/lib/apt/lists/*;\nRUN apt-get -y --no-install-recommends install php5-curl && rm -rf /var/lib/apt/lists/*;\nRUN apt-get -y --no-install-recommends install libmemcached-dev && rm -rf /var/lib/apt/lists/*;\nRUN apt-get -y --no-install-recommends install memcached php5-memcached && rm -rf /var/lib/apt/lists/*;\n\n# helper tools \nRUN apt-get -y --no-install-recommends install telnet && rm -rf /var/lib/apt/lists/*;\n\n# configure apache \nADD apache_conf/apache2.conf /etc/apache2/apache2.conf \nADD apache_conf/000-default.conf /etc/apache2/sites-enabled/000-default.conf \n\n# Install Opcache and APCu \nADD apache_conf/install_opcache_apcu.sh /var/www/scripts/ \n\n# ENV changes \nENV APACHE_RUN_USER www-data \nENV APACHE_RUN_GROUP www-data \nENV APACHE_LOG_DIR /var/log/apache2 \n\nENV MEMCACHED_LOG_DIR /var/log/memcached.log \n\nEXPOSE 80 \nEXPOSE 11211 \n\nCMD [\"/var/www/scripts/install_opcache_apcu.sh\"] \n\n"
}