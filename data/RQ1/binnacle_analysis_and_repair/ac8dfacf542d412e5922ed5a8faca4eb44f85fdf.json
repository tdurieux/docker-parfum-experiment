 -y libxml2-utils && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y build-essential && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y automake autoconf && rm -rf /var/lib/apt/lists/*;\n\n# Git 2.0\nRUN apt-get install --no-install-recommends -y software-properties-common python-software-properties && rm -rf /var/lib/apt/lists/*;\nRUN add-apt-repository ppa:git-core/ppa\nRUN apt-get update\nRUN apt-get install --no-install-recommends -y git git-core && rm -rf /var/lib/apt/lists/*;\n\n# PHP\nRUN LC_ALL=C.UTF-8 add-apt-repository ppa:ondrej/php \\\n  && apt-get install --no-install-recommends -y php && rm -rf /var/lib/apt/lists/*;\n\n# Java\nRUN apt-get install --no-install-recommends -y default-jdk && rm -rf /var/lib/apt/lists/*;\n\n# MongoDB\nRUN apt-key adv --keyserver hkp://keyserver.ubuntu.com:80 --recv 0C49F3730359A14518585931BC711F9BA15703C6 \\\n  && echo \"deb [ arch=amd64 ] http://repo.mongodb.org/apt/ubuntu trusty/mongodb-org/3.4 multiverse\" | tee /etc/apt/sources.list.d/mongodb-org-3.4.list \\\n  && apt-get update \\\n  && apt-get install --no-install-recommends -y mongodb-org && rm -rf /var/lib/apt/lists/*;\n\n# S3cmd for AWS S3 integration\nRUN apt-get install --no-install-recommends -y s3cmd && rm -rf /var/lib/apt/lists/*;\n\n# MySQL client\nRUN apt-get install --no-install-recommends -y mysql-client && rm -rf /var/lib/apt/lists/*;\n\n# PostgreSQL client\nRUN apt-get install -y --no-install-recommends wget ca-certificates \\\n  && wget -q https://www.postgresql.org/media/keys/ACCC4CF8.asc -O - | apt-key add - \\\n  && echo \"deb http://apt.postgresql.org/pub/repos/apt/ `lsb_release -cs`-pgdg main\" >> /etc/apt/sources.list.d/pgdg.list \\\n  && apt-get update \\\n  && apt-get install --no-install-recommends -y postgresql postgresql-contrib && rm -rf /var/lib/apt/lists/*;\n\n# NodeJS\nRUN rm -rf /usr/lib/node_modules \\\n  && curl -f -sL https://deb.nodesource.com/setup_6.x | bash - \\\n  && apt-get install --no-install-recommends -y nodejs && rm -rf /var/lib/apt/lists/*;\n\n# Ruby\nRUN apt-get update \\\n  && apt-get install --no-install-recommends -y ruby-dev libmagic-dev \\\n  && gpg --batch --keyserver hkp://keys.gnupg.net --recv-keys D39DC0E3 \\\n  && gpg --batch --keyserver hkp://keys.gnupg.net --recv-keys 409B6B1796C275462A1703113804BB82D39DC0E3 7D2BAF1CF37B13E2069D6956105BD0E739499BDB \\\n  && curl -f -L https://get.rvm.io | bash -s stable \\\n  && /bin/bash -l -c \". /etc/profile.d/rvm.sh && rvm install 2.3.3 && gem update && gem install --no-ri --no-rdoc nokogiri bundler\" && rm -rf /var/lib/apt/lists/*;\n\n# Clean up\nRUN rm -rf /tmp/*\nRUN rm -rf /root/.ssh\n\nENTRYPOINT [\"/bin/bash\", \"-l\", \"-c\"]\n"
}