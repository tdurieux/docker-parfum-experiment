  "repairedDockerfile": "FROM ubuntu:16.04 \nMAINTAINER Nguyen Thanh Hai \n\n#RUN DEBIAN_FRONTEND=noninteractive \nRUN apt-get update \n\nRUN apt-get -y upgrade \n\nRUN apt-get install -y --no-install-recommends nano && rm -rf /var/lib/apt/lists/*;\n\nRUN apt-get -y --no-install-recommends install wget && rm -rf /var/lib/apt/lists/*;\n\nRUN wget https://storage.googleapis.com/golang/go1.8.3.linux-amd64.tar.gz \n\nRUN tar -C /usr/local -xf go1.8.3.linux-amd64.tar.gz && rm go1.8.3.linux-amd64.tar.gz\n\nRUN echo 'export PATH=$PATH:/usr/local/go/bin' >> ~/.profile \n\nCOPY /entrypoint.sh /scripts/entrypoint.sh \n\nRUN chmod a+x /scripts/entrypoint.sh \n\nENTRYPOINT [\"/scripts/entrypoint.sh\"] \n\nRUN apt update \n\nRUN apt install --no-install-recommends -y ruby ruby-dev gcc make python-software-properties curl && rm -rf /var/lib/apt/lists/*;\n\nRUN gem install sass \n\nRUN curl -f -sL https://deb.nodesource.com/setup_8.x | bash -\n\nRUN apt install -y --no-install-recommends nodejs && rm -rf /var/lib/apt/lists/*;\n\nRUN npm install -g yarn gulp webpack && npm cache clean --force;\n\nRUN apt install --no-install-recommends -y nginx && rm -rf /var/lib/apt/lists/*;\n\nCMD [\"/usr/sbin/nginx\", \"-g\", \"daemon off;\"] \n\n\n"
