  "repairedDockerfile": "FROM ubuntu:trusty\nENV DEBIAN_FRONTEND noninteractive\n\n# nginx\nRUN apt-get update -q \\\n    && apt-get install --no-install-recommends -yf build-essential python-software-properties software-properties-common \\\n    && add-apt-repository ppa:nginx/stable \\\n    && apt-get update -q \\\n    && apt-get -y --no-install-recommends install -y curl && rm -rf /var/lib/apt/lists/*;\n\n# build nginx from source with http auth module enabled\nRUN apt-get -y --no-install-recommends install libpcre3-dev zlib1g-dev libssl-dev && rm -rf /var/lib/apt/lists/*;\n\nRUN curl -f -s https://nginx.org/download/nginx-1.6.2.tar.gz | tar -xz -C /tmp \\\n\t&& cd /tmp/nginx-1.6.2 \\\n\t&& ./configure --build=\"$(dpkg-architecture --query DEB_BUILD_GNU_TYPE)\" --with-http_ssl_module --with-http_auth_request_module && make && make install\n\n# put self signed ssl key (generated by htpasswd\nRUN apt-get -y --no-install-recommends install apache2-utils && rm -rf /var/lib/apt/lists/*;\n\nADD nginx.conf /usr/local/nginx/conf/nginx.conf\nADD data /nginx\nADD start.sh /start.sh\nRUN chmod +x /start.sh\nENV PATH /usr/local/nginx/sbin:$PATH\n\nVOLUME ['/nginx']\n\nEXPOSE 80 443\n\nCMD /start.sh"
