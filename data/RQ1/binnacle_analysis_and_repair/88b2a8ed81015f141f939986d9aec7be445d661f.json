  "repairedDockerfile": "FROM debian\n\nMAINTAINER alan.zimm@gmail.com\n\nENV DEBIAN_FRONTEND noninteractive\n\n####### GHC 7.6.3 ######################\nRUN apt-get update\nRUN apt-get install --no-install-recommends -y wget && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y bzip2 && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y libgmp-dev && rm -rf /var/lib/apt/lists/*;\n\nRUN wget https://www.haskell.org/ghc/dist/7.6.3/ghc-7.6.3-x86_64-unknown-linux.tar.bz2\n\nRUN tar xvfj ghc-7.6.3-x86_64-unknown-linux.tar.bz2 && rm ghc-7.6.3-x86_64-unknown-linux.tar.bz2\n\nRUN ln -s /usr/lib/x86_64-linux-gnu/libgmp.so /usr/lib/x86_64-linux-gnu/libgmp.so.3\n\nRUN apt-get install --no-install-recommends -y libncursesw5 gcc apt-utils && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y make && rm -rf /var/lib/apt/lists/*;\n\nRUN cd ghc-7.6.3 && ./configure --build=\"$(dpkg-architecture --query DEB_BUILD_GNU_TYPE)\"\nRUN cd ghc-7.6.3 && make install\n\n##### haskell platform 2013.2.0.0 #################\n\nRUN wget https://www.haskell.org/platform/download/2013.2.0.0/haskell-platform-2013.2.0.0.tar.gz\n\nRUN tar xvfz haskell-platform-2013.2.0.0.tar.gz && rm haskell-platform-2013.2.0.0.tar.gz\n\nRUN apt-get install --no-install-recommends -y zlib1g-dev libgl1-mesa-dev libglu1-mesa-dev freeglut3-dev && rm -rf /var/lib/apt/lists/*;\n\nRUN apt-get install --no-install-recommends -y libglc-dev libedit-dev libglw1-mesa libglw1-mesa-dev && rm -rf /var/lib/apt/lists/*;\n# RUN apt-get install -y zlib1g-dev\n\n\nRUN cd haskell-platform-2013.2.0.0 && ./configure --build=\"$(dpkg-architecture --query DEB_BUILD_GNU_TYPE)\"\nRUN cd haskell-platform-2013.2.0.0 && make\nRUN cd haskell-platform-2013.2.0.0 && make install\n\n\n# Note: to debug the partial build do the following\n# docker run -i -t alanz/haskell-platform-2013.2.0.0-64 /bin/bash\n"
