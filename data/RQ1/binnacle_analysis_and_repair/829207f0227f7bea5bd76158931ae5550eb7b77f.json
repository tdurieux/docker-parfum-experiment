  "repairedDockerfile": "# \n# ZeroMQ Message Platform \n# Builds a container for developing the Message Platform \n# \nFROM ubuntu \n\nMAINTAINER Howard Young \"https://github.com/threadx\" \nRUN apt-get update --fix-missing \n\nRUN apt-get install --no-install-recommends -y g++ && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y make && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y xutils-dev && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y libtool && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y automake && rm -rf /var/lib/apt/lists/*;\n\n# \n# Required libs \n# \nRUN apt-get install --no-install-recommends -y libzip-dev && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y libcurl4-openssl-dev && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y libxml2-dev && rm -rf /var/lib/apt/lists/*;\n\n# \n# Other Utils \n# \nRUN apt-get install --no-install-recommends -y wget && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y traceroute && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y unzip && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y git && rm -rf /var/lib/apt/lists/*;\n\n# \n# vi just doesn't cut it anymore \n# \nRUN apt-get install --no-install-recommends -y vim && rm -rf /var/lib/apt/lists/*;\n\nRUN git clone git://github.com/jedisct1/libsodium.git \nRUN cd libsodium; ./autogen.sh; ./configure --build=\"$(dpkg-architecture --query DEB_BUILD_GNU_TYPE)\" && make; make install; ldconfig\n\nRUN git clone git://github.com/zeromq/libzmq.git \nRUN cd libzmq; ./autogen.sh; ./configure --build=\"$(dpkg-architecture --query DEB_BUILD_GNU_TYPE)\" && make; make install; ldconfig\n\nRUN git clone git://github.com/zeromq/czmq.git \nRUN cd czmq; ./autogen.sh; ./configure --build=\"$(dpkg-architecture --query DEB_BUILD_GNU_TYPE)\" && make; make install; ldconfig\n\n# \n# Message Platform \n# \nRUN mkdir -p /var/log/mp \n\ncmd /bin/bash \n\n"
