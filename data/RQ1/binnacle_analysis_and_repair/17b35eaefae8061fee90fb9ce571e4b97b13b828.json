  "repairedDockerfile": "FROM ubuntu:trusty\nMAINTAINER EasyChen <easychen@gmail.com>\n\n# 添加商业软件源\n#deb http://archive.ubuntu.com/ubuntu trusty multiverse\n#deb http://archive.ubuntu.com/ubuntu trusty-updates multiverse\n\nRUN echo \"deb http://archive.ubuntu.com/ubuntu trusty multiverse\" >> /etc/apt/sources.list\nRUN echo \"deb http://archive.ubuntu.com/ubuntu trusty-updates multiverse\" >> /etc/apt/sources.list\n\n# 先更新apt-get\nRUN apt-get update && apt-get upgrade -y\n\n# 安装unrar\nRUN apt-get install --no-install-recommends unrar -y && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends p7zip-full p7zip-rar -y && rm -rf /var/lib/apt/lists/*;\n#RUN apt-get install p7zip-rar -y\n\n# 安装PHP和Apache\nRUN apt-get install --no-install-recommends apache2 -y && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends php5 libapache2-mod-php5 php5-mcrypt php5-cli php5-curl php5-gd -y && rm -rf /var/lib/apt/lists/*;\n\n\n# 安装WebApp\nRUN rm -Rf /var/www/html\n\nRUN apt-get install --no-install-recommends git -y && rm -rf /var/lib/apt/lists/*;\nRUN git clone https://github.com/easychen/KODExplorer.git  /var/www/html\n\n\n# 安装aria2\nRUN apt-get install --no-install-recommends aria2 -y && rm -rf /var/lib/apt/lists/*;\n\n#RUN mkdir cldata\nADD aria2.conf /cldata/aria2.conf\nCOPY init.sh /cldata/init.sh\nCOPY init.php /cldata/init.php\nRUN chmod +x /cldata/init.sh\n\nWORKDIR /var/www/html/comic\nVOLUME /var/www/html/comic\n\n\nEXPOSE 80 6800\n\nCMD /cldata/init.sh"
}