  "repairedDockerfile": "FROM ubuntu:18.04\n\n# Update and install some software requirements\nRUN apt-get update\nRUN apt-get upgrade -y\nRUN apt-get install --no-install-recommends -y curl && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y build-essential && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y wget && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y git && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y make && rm -rf /var/lib/apt/lists/*;\nRUN DEBIAN_FRONTEND=noninteractive apt-get --no-install-recommends install -y postgresql && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y inotify-tools && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y xz-utils && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y unzip && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y screen && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y locales && rm -rf /var/lib/apt/lists/*;\n\n# Elixir requires UTF-8\nRUN locale-gen en_US.UTF-8\nENV LANG en_US.UTF-8\nENV LANGUAGE en_US:en\nENV LC_ALL en_US.UTF-8\n\n# For some reason, installing Elixir tries to remove this file\n# and if it doesn't exist, Elixir won't install. So, we create it.\n# Thanks Daniel Berkompas for this tip.\n# http://blog.danielberkompas.com\nRUN touch /etc/init.d/couchdb\n\n# Install Node\nENV NPM_CONFIG_LOGLEVEL info\nENV NODE_VERSION 10.15.3\n\n# install Node.js with package\nRUN curl -f -sL https://deb.nodesource.com/setup_10.x | bash -\n\nRUN apt-get install --no-install-recommends -y nodejs && rm -rf /var/lib/apt/lists/*;\n\n# download and install Erlang package\nRUN wget https://packages.erlang-solutions.com/erlang-solutions_1.0_all.deb \\\n && dpkg -i erlang-solutions_1.0_all.deb \\\n && apt-get update\n\n# install Erlang and Elixir\nENV ERLANG_VERSION 1:22.0-1\nENV ELIXIR_VERSION 1.8.2-1\n\nRUN apt-get install --no-install-recommends -y esl-erlang=$ERLANG_VERSION && rm -rf /var/lib/apt/lists/*;\n\nRUN apt-mark hold esl-erlang\n\nRUN apt-get install --no-install-recommends -y elixir=$ELIXIR_VERSION && rm -rf /var/lib/apt/lists/*;\n\nRUN rm erlang-solutions_1.0_all.deb\n\n# install Hex\nRUN mix local.hex --force\n\nENV PHOENIX_VERSION 1.4.6\n\n# install the Phoenix Mix archive\nRUN mix archive.install --force hex phx_new $PHOENIX_VERSION\n\nRUN mix hex.info\n\n# include Dockerize to help launching containers\nRUN wget https://github.com/jwilder/dockerize/releases/download/v0.6.1/dockerize-linux-amd64-v0.6.1.tar.gz\nRUN tar -C /usr/local/bin -xzvf dockerize-linux-amd64-v0.6.1.tar.gz && rm dockerize-linux-amd64-v0.6.1.tar.gz\n\n# include wait-for-it.sh\nRUN curl -f -o /bin/wait-for-it.sh https://raw.githubusercontent.com/vishnubob/wait-for-it/master/wait-for-it.sh\nRUN chmod a+x /bin/wait-for-it.sh\n\n# install headless Chrome compatible with puppeteer\nRUN apt-get update && apt-get install --no-install-recommends -yq libgconf-2-4 && rm -rf /var/lib/apt/lists/*;\nRUN apt-get update && apt-get install -y wget --no-install-recommends \\\n  && wget -q -O - https://dl-ssl.google.com/linux/linux_signing_key.pub | apt-key add - \\\n  && sh -c 'echo \"deb [arch=amd64] http://dl.google.com/linux/chrome/deb/ stable main\" >> /etc/apt/sources.list.d/google.list' \\\n  && apt-get update \\\n  && apt-get install -y google-chrome-unstable fonts-ipafont-gothic fonts-wqy-zenhei fonts-thai-tlwg fonts-kacst fonts-freefont-ttf \\\n    --no-install-recommends \\\n  && rm -rf /var/lib/apt/lists/* \\\n  && apt-get purge --auto-remove -y curl \\\n  && rm -rf /src/*.deb\n\n"
}