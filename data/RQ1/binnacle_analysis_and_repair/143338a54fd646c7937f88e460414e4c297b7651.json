  "repairedDockerfile": "FROM ubuntu:trusty\n\nRUN apt-get update\n\nRUN apt-get -y --no-install-recommends install software-properties-common && rm -rf /var/lib/apt/lists/*;\n\n# Add Ruby-2.3.3 repository\nRUN apt-add-repository -y ppa:brightbox/ruby-ng\n\n# Add MariaDB repository\nRUN apt-key adv --recv-keys --keyserver hkp://keyserver.ubuntu.com:80 0xcbcb082a1bb943db\nRUN add-apt-repository 'deb http://www.ftp.saix.net/DB/mariadb/repo/10.0/ubuntu trusty main'\n\nRUN apt-get update\n\n# Fetch updates\nRUN apt-get -y --no-install-recommends install wget && rm -rf /var/lib/apt/lists/*;\n\n# Install Ruby 2.3.*\nRUN echo \"20171211\" && apt-get -y --no-install-recommends install ruby2.3 ruby2.3-dev && rm -rf /var/lib/apt/lists/*;\n\n# Install misc dependencies\nRUN apt-get -y --no-install-recommends install build-essential patch zlib1g-dev liblzma-dev libxml2-dev libxslt1-dev libfontconfig1 libfreetype6 git curl unzip && rm -rf /var/lib/apt/lists/*;\n\n# Install MariaDB\nRUN apt-get -y --no-install-recommends install mariadb-server libmariadbclient-dev && rm -rf /var/lib/apt/lists/*;\n\n# Install bundler\nRUN gem install bundler\n\n# Install xvfb for headless selenium\nRUN apt-get -y --no-install-recommends install xvfb && rm -rf /var/lib/apt/lists/*;\n\n# Install NodeJS\nRUN curl -f -sL https://deb.nodesource.com/setup_5.x | bash -\nRUN apt-get -y --no-install-recommends install nodejs && rm -rf /var/lib/apt/lists/*;\n\nRUN apt-get -yq --no-install-suggests --no-install-recommends --force-yes install libqtwebkit-dev libqtwebkit4 && rm -rf /var/lib/apt/lists/*;\nRUN apt-get -y --no-install-recommends install gstreamer1.0-plugins-base gstreamer1.0-tools gstreamer1.0-x && rm -rf /var/lib/apt/lists/*;\n\nADD xvfb /etc/init.d/xvfb\nRUN chmod a+x /etc/init.d/xvfb\n"
