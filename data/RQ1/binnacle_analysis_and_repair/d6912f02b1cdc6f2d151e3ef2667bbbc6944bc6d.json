  "repairedDockerfile": "FROM ubuntu:10.04\n\nRUN apt-get update\nRUN apt-get -y --no-install-recommends install curl mercurial && rm -rf /var/lib/apt/lists/*;\nRUN apt-get -y --no-install-recommends install make gcc ed bison && rm -rf /var/lib/apt/lists/*;\nRUN apt-get -y --no-install-recommends install emacs22-nox && rm -rf /var/lib/apt/lists/*;\nRUN apt-get -y --no-install-recommends install git-core && rm -rf /var/lib/apt/lists/*;\n\nRUN adduser gopher\n\nENV GOROOT /home/gopher/go\nENV GOBIN /home/gopher/go/bin\n\nWORKDIR /home/gopher\nRUN curl -f --remote-name http://bradfitz-public.s3-website-us-east-1.amazonaws.com/go.tar.gz\nRUN tar -zxvf go.tar.gz && rm go.tar.gz\nRUN rm go.tar.gz\nRUN mkdir -p $GOBIN\n\nENV GOVERS afd6198fb082\nRUN cd $GOROOT && hg update -C $GOVERS\nRUN echo $GOVERS > $GOROOT/VERSION\n\nRUN chown -R gopher.gopher $GOROOT\nRUN apt-get -y --no-install-recommends install libc6-dev-i386 && rm -rf /var/lib/apt/lists/*;\nRUN apt-get -y --no-install-recommends install sudo && rm -rf /var/lib/apt/lists/*;\nRUN echo \"gopher ALL=(ALL) NOPASSWD:ALL\" >> /etc/sudoers\n\nUSER gopher\nENV GOARCH 386\nRUN cd $GOROOT/src && ./make.bash\n\nENV CAMVERS e7b00b6e2c3955350d1c714637f16e8b985511aa\nRUN cd /home/gopher && git clone https://github.com/bradfitz/camlistore.git\nRUN cd /home/gopher/camlistore && git reset --hard $CAMVERS\n\nENV PATH /home/gopher/go/bin:/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin\nENV HOME /home/gopher\nENTRYPOINT /bin/bash\n\n"
