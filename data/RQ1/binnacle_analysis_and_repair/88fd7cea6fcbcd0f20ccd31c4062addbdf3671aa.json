  "repairedDockerfile": "FROM nvidia/cuda:7.5-cudnn5-devel\nMAINTAINER Ly Nguyen <lynguyen@amazon.com>\n\n# OPENCV\nRUN apt-mark hold libcudnn5 libcudnn5-dev\nRUN apt-get update && apt-get -y upgrade\nRUN apt-get install --no-install-recommends -y libopencv-dev && rm -rf /var/lib/apt/lists/*;\n\n# BLAS\nRUN apt-get install --no-install-recommends -y libatlas-base-dev && rm -rf /var/lib/apt/lists/*;\n\n# PYTHON2\nRUN apt-get install --no-install-recommends -y python-setuptools python-pip python-dev unzip gfortran && rm -rf /var/lib/apt/lists/*;\nRUN pip install --no-cache-dir numpy nose scipy\n\n# PYTHON3\nRUN apt-get install --no-install-recommends -y python3-setuptools python3-pip && rm -rf /var/lib/apt/lists/*;\nRUN pip3 install --no-cache-dir numpy nose scipy && ln -s -f /usr/local/bin/nosetests-3.4 /usr/local/bin/nosetests3\n\n# TESTDEPS\nRUN apt-get install --no-install-recommends -y libgtest-dev cmake wget && rm -rf /var/lib/apt/lists/*;\nRUN cd /usr/src/gtest && cmake CMakeLists.txt && make && cp *.a /usr/lib\nRUN pip install --no-cache-dir nose cpplint 'pylint==1.4.4' 'astroid==1.3.6'\n\n# MAVEN\nRUN apt-get install --no-install-recommends -y software-properties-common && rm -rf /var/lib/apt/lists/*;\nRUN add-apt-repository ppa:webupd8team/java -y\nRUN apt-get update\nRUN echo \"oracle-java8-installer shared/accepted-oracle-license-v1-1 select true\" | debconf-set-selections\nRUN apt-get install --no-install-recommends -y oracle-java8-installer && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y oracle-java8-set-default && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y maven && rm -rf /var/lib/apt/lists/*;\n\n# R\nRUN apt-get install --no-install-recommends -y software-properties-common r-base-core libcurl4-openssl-dev libssl-dev libxml2-dev && rm -rf /var/lib/apt/lists/*;\nRUN apt-key adv --keyserver keyserver.ubuntu.com --recv-keys E084DAB9\nRUN add-apt-repository -y ppa:marutter/rdev\nRUN apt-get update && apt-get -y upgrade\nRUN DEBIAN_FRONTEND=noninteractive apt-get --no-install-recommends -y -o Dpkg::Options::=\"--force-confdef\" -o install r-base r-base-dev && rm -rf /var/lib/apt/lists/*;\nRUN Rscript -e \"install.packages('devtools', repo = 'https://cran.rstudio.com')\"\n"
