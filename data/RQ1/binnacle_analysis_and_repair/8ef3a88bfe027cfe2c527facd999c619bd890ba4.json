  "repairedDockerfile": "FROM daocloud.io/ubuntu\nMAINTAINER zhengwen \"mail.zhengwen@gmail.com\"\nRUN sed -i 's/http:\\/\\/archive.ubuntu.com\\/ubuntu\\//http:\\/\\/mirrors.163.com\\/ubuntu\\//g' /etc/apt/sources.list\nRUN apt-get update\nRUN apt-get install --no-install-recommends -y python2.7 python-dev python-distribute python-pip ipython && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y iputils-ping && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y net-tools && rm -rf /var/lib/apt/lists/*;\n\nADD pip.conf ~/.pip/\nRUN pip install --no-cache-dir wheel\nRUN pip install --no-cache-dir requests\nRUN apt-get install --no-install-recommends -y libffi-dev libxml2-dev libxslt-dev lib32z1-dev libssl-dev && rm -rf /var/lib/apt/lists/*;\nRUN pip install --no-cache-dir lxml scrapy scrapyd\nRUN pip install --no-cache-dir redis\nRUN pip install --no-cache-dir pymongo\nRUN pip install --no-cache-dir scrapy-redis\n\nRUN mkdir /usr/local/spider\nRUN mkdir -p /data/spider/\nRUN apt-get install --no-install-recommends -y supervisor && rm -rf /var/lib/apt/lists/*;\nRUN echo_supervisord_conf > /etc/supervisor/conf.d/supervisord.conf\nRUN sed -i '$a [supervisord]\\nnodaemon=true\\n[program:scrapyd]\\ncommand=scrapyd\\ndirectory=/usr/local/spider' /etc/supervisor/conf.d/supervisord.conf\nENV SPIDER_ENV product\n\nRUN mkdir /etc/scrapyd\nADD scrapyd.conf /etc/scrapyd/scrapyd.conf\n\nEXPOSE 6800\nCMD [\"/usr/bin/supervisord\"]\t\n"
