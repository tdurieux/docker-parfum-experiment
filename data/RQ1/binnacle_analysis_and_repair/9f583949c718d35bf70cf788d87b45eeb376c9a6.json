  "repairedDockerfile": "FROM node:10.15-stretch\n\nMAINTAINER Marco Raddatz\n\n# Set environment variables\nENV DEBIAN_FRONTEND noninteractive\nENV TERM xterm\n\n# Install dependencies and tools\nRUN apt-get update; \\\n    apt-get install --no-install-recommends -y apt-utils apt-transport-https; rm -rf /var/lib/apt/lists/*; \\\n    apt-get install --no-install-recommends -y curl wget; \\\n    apt-get install --no-install-recommends -y libnss-mdns avahi-discover libavahi-compat-libdnssd-dev libkrb5-dev; \\\n    apt-get install --no-install-recommends -y ffmpeg; \\\n    apt-get install --no-install-recommends -y nano vim\n\n# Install latest Homebridge\n# -------------------------------------------------------------------------\n# You can force a specific version by setting HOMEBRIDGE_VERSION\n# See https://github.com/marcoraddatz/homebridge-docker#homebridge_version\n# -------------------------------------------------------------------------\nRUN npm install -g homebridge --unsafe-perm && npm cache clean --force;\n\n# MISC settings\nCOPY avahi-daemon.conf /etc/avahi/avahi-daemon.conf\n\nUSER root\nRUN mkdir -p /var/run/dbus\n\nADD image/run.sh /root/run.sh\n\n# Run container\nEXPOSE 5353 51826\nCMD [\"/root/run.sh\"]\n"
