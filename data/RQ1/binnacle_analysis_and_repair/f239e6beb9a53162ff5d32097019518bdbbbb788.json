  "repairedDockerfile": "from ubuntu:14.04\nmaintainer atupal me@atupal.org\n\n# add aliyun mirror\nrun perl -p -i.orig -e 's/archive.ubuntu.com/mirrors.aliyun.com\\/ubuntu/' /etc/apt/sources.list\nrun apt-get update\nrun apt-get install --no-install-recommends -y build-essential git && rm -rf /var/lib/apt/lists/*;\n\nrun apt-get install --no-install-recommends -y redis-server && rm -rf /var/lib/apt/lists/*;\nrun apt-get install --no-install-recommends -y python python-dev python-setuptools && rm -rf /var/lib/apt/lists/*;\nrun easy_install pip\n\nrun apt-get install --no-install-recommends -y libxml2-dev libxslt1-dev && rm -rf /var/lib/apt/lists/*;\n\n# fix /usr/bin/ld: cannot find -lz\nrun apt-get install --no-install-recommends -y zlib1g-dev && rm -rf /var/lib/apt/lists/*;\n\n# numpy dependency\nrun apt-get install --no-install-recommends -y libblas-dev && rm -rf /var/lib/apt/lists/*;\nrun apt-get install --no-install-recommends -y libatlas-dev liblapack-dev && rm -rf /var/lib/apt/lists/*;\n\n# run pip install\nadd ./requirements.txt /home/docker/code/requirements.txt\nadd . /home/docker/code\nrun pip install --no-cache-dir -r /home/docker/code/requirements.txt -i http://pypi.douban.com/simple\n\nWORKDIR /home/docker/code\ncmd [\"celery\", \"-A\", \"celeryapp\", \"worker\", \"-c\", \"500\", \"-P\", \"gevent\", \"--soft-time-limit\", \"10\"]\n"
