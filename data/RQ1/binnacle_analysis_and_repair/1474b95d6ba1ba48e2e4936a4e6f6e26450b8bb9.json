  "repairedDockerfile": "FROM ubuntu:14.04\nMAINTAINER Michael Neale <mneale@cloudbees.com\n\n# First, let us install Jenkins - as per https://github.com/cloudbees/jenkins-docker\nRUN echo \"0.12\" > .version\nRUN apt-get update\nRUN echo deb http://pkg.jenkins-ci.org/debian binary/ >> /etc/apt/sources.list\nRUN apt-get install --no-install-recommends -y wget && rm -rf /var/lib/apt/lists/*;\nRUN wget -q -O - https://pkg.jenkins-ci.org/debian/jenkins-ci.org.key | apt-key add -\nRUN apt-get update\nRUN apt-get install --no-install-recommends -y jenkins && rm -rf /var/lib/apt/lists/*;\n\n\n# now we install docker in docker - thanks to https://github.com/jpetazzo/dind\nRUN apt-get install --no-install-recommends -qqy iptables ca-certificates && rm -rf /var/lib/apt/lists/*;\nADD https://get.docker.io/builds/Linux/x86_64/docker-latest /usr/local/bin/docker\nADD ./wrapdocker /usr/local/bin/wrapdocker\nRUN chmod +x /usr/local/bin/docker /usr/local/bin/wrapdocker\nVOLUME /var/lib/docker\n\nRUN apt-get install --no-install-recommends -y git && rm -rf /var/lib/apt/lists/*;\n\nCMD wrapdocker && exec java -jar /usr/share/jenkins/jenkins.war\n"
