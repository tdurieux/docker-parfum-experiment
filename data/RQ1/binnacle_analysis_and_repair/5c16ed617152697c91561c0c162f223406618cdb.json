  "repairedDockerfile": "FROM ubuntu:16.04\n\nLABEL maintainer=\"openvidu@gmail.com\"\n\nUSER root\n\nRUN apt-get update && apt-get -y upgrade\n\nRUN apt-get install --no-install-recommends -y software-properties-common && apt-get install -y --no-install-recommends apt-utils && rm -rf /var/lib/apt/lists/*;\n\n# Install Kurento Media Server (KMS)\nRUN echo \"deb [arch=amd64] http://ubuntu.openvidu.io/6.10.0 xenial kms6\" | tee /etc/apt/sources.list.d/kurento.list \\\n\t&& apt-key adv --keyserver keyserver.ubuntu.com --recv-keys 5AFA7A83 \\\n\t&& apt-get update \\\n\t&& apt-get -y --no-install-recommends install kurento-media-server && rm -rf /var/lib/apt/lists/*;\nRUN sed -i \"s/DAEMON_USER=\\\"kurento\\\"/DAEMON_USER=\\\"root\\\"/g\" /etc/default/kurento-media-server\n\n# Install Node\nRUN apt-get update && apt-get install --no-install-recommends -y curl && rm -rf /var/lib/apt/lists/*;\nRUN curl -f -sL https://deb.nodesource.com/setup_10.x | bash - && apt-get install --no-install-recommends -y nodejs && rm -rf /var/lib/apt/lists/*;\n\n# Java 8\nRUN apt-get install --no-install-recommends -y openjdk-8-jdk-headless && rm -rf /var/lib/apt/lists/*;\n\n# Maven\nRUN apt-get install --no-install-recommends -y maven && rm -rf /var/lib/apt/lists/*;\n\n# git\nRUN apt-get install --no-install-recommends -y git && rm -rf /var/lib/apt/lists/*;\n\n# http-server\nRUN npm install -g http-server@latest && npm cache clean --force;\n\n# sudo\nRUN apt-get -y --no-install-recommends install sudo && rm -rf /var/lib/apt/lists/*;\n\n# ffmpeg (for ffprobe)\nRUN add-apt-repository ppa:jonathonf/ffmpeg-4\nRUN apt-get update\nRUN apt-get install --no-install-recommends -y ffmpeg && rm -rf /var/lib/apt/lists/*;\n\n# Cleanup\nRUN rm -rf /var/lib/apt/lists/*\nRUN apt-get autoremove --purge -y\n\nCOPY entrypoint.sh /entrypoint.sh\nRUN [\"chmod\", \"+x\", \"/entrypoint.sh\"]\n\nENTRYPOINT [\"/entrypoint.sh\"]\n"
