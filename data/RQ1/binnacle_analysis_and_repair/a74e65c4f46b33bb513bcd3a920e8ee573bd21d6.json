  "repairedDockerfile": "# start with a powershell image.\nFROM microsoft/powershell\n\n# install dotnet core sdk.\n# steps from https://www.microsoft.com/net/core#linuxubuntu\nRUN curl -f https://packages.microsoft.com/keys/microsoft.asc | gpg --batch --dearmor > microsoft.gpg\nRUN mv microsoft.gpg /etc/apt/trusted.gpg.d/microsoft.gpg\nRUN sh -c 'echo \"deb [arch=amd64] https://packages.microsoft.com/repos/microsoft-ubuntu-xenial-prod xenial main\" > /etc/apt/sources.list.d/dotnetdev.list'\nRUN apt-get -y --no-install-recommends install apt-transport-https && rm -rf /var/lib/apt/lists/*;\nRUN apt-get -y update\nRUN apt-get -y --no-install-recommends install dotnet-dev-1.1 && rm -rf /var/lib/apt/lists/*;\nRUN apt-get -y --no-install-recommends install dotnet-sdk-2.1 && rm -rf /var/lib/apt/lists/*;\n\n# install phantomjs\n# steps from https://www.vultr.com/docs/how-to-install-phantomjs-on-ubuntu-16-04\nRUN apt-get install --no-install-recommends build-essential chrpath libssl-dev libxft-dev libfreetype6-dev libfreetype6 libfontconfig1-dev libfontconfig1 -y && rm -rf /var/lib/apt/lists/*;\nRUN curl -f https://bitbucket.org/ariya/phantomjs/downloads/phantomjs-2.1.1-linux-x86_64.tar.bz2 -O -L\nRUN ls -lash phantomjs-2.1.1-linux-x86_64.tar.bz2\nRUN tar xvjf phantomjs-2.1.1-linux-x86_64.tar.bz2 -C /usr/local/share/ && rm phantomjs-2.1.1-linux-x86_64.tar.bz2\nRUN ln -s /usr/local/share/phantomjs-2.1.1-linux-x86_64/bin/phantomjs /usr/local/bin/\n\n# install python (required by casperjs)\nRUN apt-get install --no-install-recommends python -y && rm -rf /var/lib/apt/lists/*;\n\n# install casperjs\nRUN curl -f https://github.com/n1k0/casperjs/tarball/1.0.3 -o casperjs-1.0.3.tar.gz -L\nRUN tar xvf casperjs-1.0.3.tar.gz && rm casperjs-1.0.3.tar.gz\nENV PATH \"$PATH:/casperjs-casperjs-76fc831/bin\"\n# patch bootstrap.js\nCOPY bootstrap.js /casperjs-casperjs-76fc831/bin\n\n# install casperjs 1.1\nRUN curl -f https://github.com/casperjs/casperjs/archive/1.1.4-1.tar.gz -o casperjs-1.1.4-1.tar.gz -L\nRUN tar xvf casperjs-1.1.4-1.tar.gz && rm casperjs-1.1.4-1.tar.gz\nENV CASPERJS11_BIN /casperjs-1.1.4-1/bin\n\n# Add debugging tools.\nRUN apt-get install --no-install-recommends git vim -y && rm -rf /var/lib/apt/lists/*;\n\n# install GDI+ API compatible implementation for cross-plaform System.Drawing\nRUN apt-get install --no-install-recommends libgdiplus -y && rm -rf /var/lib/apt/lists/*;\n"
