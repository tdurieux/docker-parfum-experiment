  "repairedDockerfile": "FROM ubuntu:14.04.5\n\n# Because there is no package cache in the image, you need to run:\nRUN apt-get update\n\n# Install nodejs\nRUN apt-get install --no-install-recommends curl -y && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends python-software-properties -y && rm -rf /var/lib/apt/lists/*;\nRUN curl -f -sL https://deb.nodesource.com/setup_6.x | bash -\nRUN apt-get update\nRUN apt-get install --no-install-recommends -y nodejs && rm -rf /var/lib/apt/lists/*;\n\n# Install smartwrap\nRUN apt-get install --no-install-recommends git -y && rm -rf /var/lib/apt/lists/*;\nRUN git clone https://www.github.com/tecfu/smartwrap\n\n# Install grunt\nRUN npm install grunt-cli -g && npm cache clean --force;\n\n# Install dev dependencies\nWORKDIR /smartwrap\nRUN npm install && npm cache clean --force;\n\n# Run unit tests\nRUN node -v\nRUN npm run test\n"
