  "repairedDockerfile": "# Autolab - autograding docker image\n\nFROM ubuntu:14.04\nMAINTAINER Mihir Pandya <mihir.m.pandya@gmail.com>\n\nRUN apt-get update\nRUN apt-get install --no-install-recommends -y gcc && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y make && rm -rf /var/lib/apt/lists/*;\nRUN apt-get install --no-install-recommends -y build-essential && rm -rf /var/lib/apt/lists/*;\n\n# Install autodriver\nWORKDIR /home\nRUN useradd autolab\nRUN useradd autograde\nRUN mkdir autolab autograde output\nRUN chown autolab:autolab autolab\nRUN chown autolab:autolab output\nRUN chown autograde:autograde autograde\nRUN apt-get install --no-install-recommends -y git && rm -rf /var/lib/apt/lists/*;\nRUN git clone https://github.com/autolab/Tango.git\nWORKDIR Tango/autodriver\nRUN make clean && make\nRUN cp autodriver /usr/bin/autodriver\nRUN chmod +s /usr/bin/autodriver\n\n# Clean up\nWORKDIR /home\nRUN apt-get remove -y git\nRUN apt-get -y autoremove\nRUN rm -rf Tango/\n\n# Check installation\nRUN ls -l /home\nRUN which autodriver\n"
