RUN VER=`cat VERSION` && wget https://storage.googleapis.com/tensorflow/libtensorflow/libtensorflow-cpu-linux-x86_64-$VER.tar.gz -O /tmp/tflib.tgz && tar xvf /tmp/tflib.tgz -C /usr/local ./lib/libtensorflow.so ./lib/libtensorflow_framework.so && touch /usr/local/lib/libtensorflow.so && touch /usr/local/lib/libtensorflow_framework.so && ldconfig && rm /tmp/tflib.tgz
